VPATH = ../shared
CC = gcc
CXX = g++
CFLAGS = -Wall -g -O3 -D__64BIT__ -fPIC
CXXFLAGS=-std=c++0x $(CFLAGS)


OBJECTS = \
	lsqfit.o \
	lib_snowmodel.o \
	lib_iec61853.o \
	lib_cec6par.o \
	lib_financial.o \
	lib_geothermal.o \
