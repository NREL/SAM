            "Label": "Time series critical load data",
            "Units": "kWac",
            "Group": "Grid Outage",
            "IndexLabels": "",
            "Flags": 2.0,
            "DefaultValue": [
                0.0
            ],
            "UIObject": "DataArray",
            "sscVariableName": "",
            "sscVariableValue": ""
        },
        "grid_outage": {
            "Version": 4.0,
            "Type": 2.0,
            "Label": "Grid outage",
            "Units": " ",
            "Group": "Grid Outage",
            "IndexLabels": "",
            "Flags": 2.0,
            "DefaultValue": [
                0.0
            ],
            "UIObject": "DataArray",
            "sscVariableName": "",
            "sscVariableValue": ""
        },
        "run_resiliency_calcs": {
            "Version": 4.0,
            "Type": 1.0,
            "Label": "Enable resilience calculations for every timestep",
            "Units": "0/1",
            "Group": "Grid Outage",
            "IndexLabels": "Meet critical load during outage time steps (grid outage data required)|Calculate hours of autonomy for a hypothetical outage (grid outage data optional)",
            "Flags": 3.0,
            "DefaultValue": 0.0,
            "UIObject": "RadioChoice",
            "sscVariableName": "",
            "sscVariableValue": ""
        }
    },
    "Equations": [],
    "Callbacks": [
        "on_load{'Grid Outage'} = define() {",
        "\ton_change{'normalize_to_utility_bill'}();",
        "\ton_change{'crit_load_type'}();",
        "\tproperty('crit_load_escalation','FixedLength',${analysis_period});",
        "\tcheck_outage_soc();",
        "\tfuel_cell_check();",
        "\tcritical_load_message();",
        "\tgrid_outage_message();\r",
        "\tenable_vars();\r",
        "};",
        "",
        "on_change{'grid_outage'} = define()",
        "{",
        "\tgrid_outage_message();",
        "\tcritical_load_message();",
        "};",
        "",
        "on_change{'crit_load_type'} = define() {",
        "\tenable( 'crit_load_user_data', ${crit_load_type});",
        "\tenable( 'crit_load_pct', !${crit_load_type});",
        "\tenable_vars();\r",
        "\tload_error_messages(); // function defined in Electric Load callback",
        "};",
        "",
        "on_change{'crit_load_pct'} = define() {",
        "\tfuel_cell_check();",
        "\tcritical_load_message();",
        "\tgrid_outage_message();",
        "\tenable_vars();",
        "\tcheck_grid_outage();\t",
        "};",
        "",
        "on_change{'crit_load_user_data'} = define() {",
        "\tfuel_cell_check();",
        "\tcritical_load_message();",
        "\tgrid_outage_message();\r",
        "\tenable_vars();",
        "\tcheck_grid_outage();\r",
        "\tload_error_messages(); // function defined in Electric Load callback",
        "};",
        "",
        "on_change{'run_resiliency_calcs'} = define() {",
        "\tcritical_load_message();",
        "};",
        "",
        "on_change{'batt_minimum_outage_SOC'} = define()",
        "{ ",
        "\tcheck_outage_soc(); ",
        "};",
        "",
        "function is_crit()",
        "{",
        "\tif ( ${crit_load_type} == 0 )",
        "\t{",
        "\t\treturn ${crit_load_pct} > 0;",
        "\t}",
        "\telse",
        "\t{",
        "\t\treturn sum(${crit_load_user_data}) > 0;",
        "\t}\t",
        "}",
        "",
        "function fuel_cell_check() ",
        "{",
        "\tif ( technology() == 'Fuel Cell' && is_crit() && ${en_batt} == 0 )",
        "\t{",
        "\t\tmsgbox('Critical Load Requires Battery!\\nYou have specified a critical load without enabling the battery on the Battery Storage page. To continue, either set the critical load percent and time series data to zero, or enable the battery.');",
        "\t}",
        "}",
        "",
        "function critical_load_message()",
        "{",
        "\tmsg = '';",
        "\tclr = 'Black';",
        "\tif ( is_crit() )",
        "\t{",
        "\t\tif ( is_grid_outage() ) // is_grid_outage() defined on Grid Outage form",
        "\t\t{",
        "\t\t\tmsg = 'Grid outage is specified. You can choose to run the model to meet critical loads during outage time steps, or to calculate hours of autonomy your system design would provide given the grid outages.';",
        "\t\t\tclr = 'Black';",
        "\t\t}",
        "\t\telseif ( value('run_resiliency_calcs') == 0) ",
        "\t\t{",
        "\t\t\tmsg = 'Grid outage data is required to model critical load during outage time steps. Click Edit Array below to specify grid outage data, or choose the Calculate Hours of Autonomy option.';",
        "\t\t\tclr = 'Red';",
        "\t\t}",
        "\t\telse",
        "\t\t{",
        "\t\t\tmsg = 'No grid outage data is specified. SAM will calculate the hours of autonomy for a hypothetical outage based on your system design.';",
        "\t\t\tclr = 'Black';",
        "\t\t}",
        "\t}",
        "\telse",
        "\t{",
        "\t\tmsg = 'Specify a critical load percentage or time series critical load data to model grid outages.';",
        "\t\tclr = 'Red';",
        "\t}\t",
        "\tproperty('lbl_critical_load','Caption',msg);",
        "\tproperty('lbl_critical_load','TextColour',clr);",
        "}",
        "",
        "function check_grid_outage()",
        "{",
        "\tif ( !is_crit() && is_grid_outage() )",
        "\t{",
        "\t\tok = yesno('Grid outage specified with no critical load.\\nYou have specified grid outages with no critical load. Either specify a critical load or remove grid outages. Would you like SAM to automatically set grid outages to zero?');",
        "\t\tif ( ok ) ",
        "\t\t{",
        "\t\t\tarr = [];",
        "\t\t\tfor ( i=0; i<#${grid_outage}; i++ )",
        "\t\t\t{",
        "\t\t\t\tarr[i] = 0;",
        "\t\t\t}",
        "\t\t\tvalue('grid_outage',arr);",
        "\t\t\tgrid_outage_message();",
        "\t\t}",
        "\t\telse",
        "\t\t{",
        "\t\t\tmsgbox('To set the grid outage to zero, temporarily set the critical load percent to a number greater than zero and click Edit Array to edit the grid outage table.');",
        "\t\t}",
        "\t}",
        "}",
        "",
        "function enable_vars()",
        "{",
        "\tif ( technology() == 'Fuel Cell' ) ",
        "\t{",
        "\t\tenable('grid_outage', ( is_crit() && ${en_batt} ));",
        "\t\tenable('run_resiliency_calcs', ( is_crit() && ${en_batt} )); ",
        "\t\tenable('batt_minimum_outage_SOC', ( is_crit() && ${en_batt} ));",
        "\t\tenable('crit_load_escalation', ( is_crit() && ${en_batt} ));",
        "\t}",
        "\telse",
        "\t{",
        "\t\tenable( 'grid_outage', is_crit() );",
        "\t\tenable( 'run_resiliency_calcs', is_crit() ); ",
        "\t\tenable('batt_minimum_outage_SOC', is_crit() );",
        "\t\tenable('crit_load_escalation', is_crit() );",
        "\t}",
        "\tif ( !is_crit() ) ",
        "\t{",
        "\t\tvalue('run_resiliency_calcs', 0); ",
        "\t}",
        "}",
        "",
        "function is_grid_outage()",
        "{",
        "\treturn sum(${grid_outage}) > 0;",
        "}",
        "",
        "function grid_outage_message()",
        "{",
        "\tmessage = '';",
        "\tcolor = 'Black';",
        "\tif ( !is_crit() && is_grid_outage() )",
        "\t{",
        "\t\tmessage = 'Grid outages must be set to zero for all time steps if there is not a critical load. Temporarily set the critical load percent to a number greater than 0 to edit the Grid Outage data.';",
        "\t\tcolor = 'Red';",
        "\t}",
        "\tproperty( 'lbl_grid_outage','Caption',message );",
        "\tproperty( 'lbl_grid_outage','TextColour',color );",
        "}",
        "",
        "function check_outage_soc()",
        "{",
        "\tshow_outage_soc_warning = false;",
        "\tsoc = value('batt_minimum_outage_SOC');",
        "\tif ( (soc <= 0.0) )",
        "\t{",
        "\t\tshow_outage_soc_warning = true;",
        "\t}",
        "\tshow('zero_outage_soc_warning', show_outage_soc_warning);",
        "\trefresh();",
        "}\r",
        ""
    ]
}