    "Equations": {
        "1": "equations{ 'ui_net_capital_cost' } = define() {",
        "2": "    cost_installed = ${total_installed_cost};",
        "3": "\tibi = ${ibi_fed_amount} ",
        "4": "\t\t+ ${ibi_sta_amount}",
        "5": "\t\t+ ${ibi_uti_amount}",
        "6": "\t\t+ ${ibi_oth_amount};",
        "7": "\tibi_fed = ${ibi_fed_percent}/100 * cost_installed;",
        "8": "\tibi_fed = min(ibi_fed,${ibi_fed_percent_maxvalue});",
        "9": "\tibi_sta = ${ibi_sta_percent}/100 * cost_installed;",
        "10": "\tibi_sta = min(ibi_sta,${ibi_sta_percent_maxvalue});",
        "11": "\tibi_uti = ${ibi_uti_percent}/100 * cost_installed;",
        "12": "\tibi_uti = min(ibi_uti,${ibi_uti_percent_maxvalue});",
        "13": "\tibi_oth = ${ibi_oth_percent}/100 * cost_installed;",
        "14": "\tibi_oth = min(ibi_oth,${ibi_oth_percent_maxvalue});",
        "15": "\tibi = ibi + ibi_fed + ibi_sta + ibi_uti + ibi_oth;\t",
        "16": "\t",
        "17": "\tcapacity = ${system_capacity}*1000; //kW to W",
        "18": "\tcbi_fed = capacity * ${cbi_fed_amount};",
        "19": "\tcbi_fed = min(cbi_fed,${cbi_fed_maxvalue});",
        "20": "\tcbi_sta = capacity * ${cbi_sta_amount};",
        "21": "\tcbi_sta = min(cbi_sta,${cbi_sta_maxvalue});",
        "22": "\tcbi_uti = capacity * ${cbi_uti_amount};",
        "23": "\tcbi_uti = min(cbi_uti,${cbi_uti_maxvalue});",
        "24": "\tcbi_oth = capacity * ${cbi_oth_amount};",
        "25": "\tcbi_oth = min(cbi_oth,${cbi_oth_maxvalue});",
        "26": "\tcbi = cbi_fed + cbi_sta + cbi_uti + cbi_oth;",
        "27": "\t",
        "28": "\tc = cost_installed - ibi - cbi;\t",
        "29": "\treturn c; ",
        "30": "};",
        "31": "",
        "32": "equations{ 'loan_amount' } = define() {",
        "33": "\treturn ${ui_net_capital_cost}*${debt_fraction}/100; };",
        "34": "",
        "35": "equations{ 'ui_wacc' } = define() {",
        "36": "\treturn 100*(",
        "37": "( ",
