        "ui_wacc": {
            "Version": 4.0,
            "Type": 1.0,
            "Label": "WACC",
            "Units": "%",
            "Group": "Residential Loan",
            "IndexLabels": "",
            "Flags": 8.0,
            "DefaultValue": 0.0,
            "UIObject": "Numeric",
            "sscVariableName": "",
            "sscVariableValue": ""
        }
    },
    "Equations": [
        "equations{ 'ui_net_capital_cost' } = define() {",
        "    cost_installed = ${total_installed_cost};",
        "\tibi = ${ibi_fed_amount} ",
        "\t\t+ ${ibi_sta_amount}",
        "\t\t+ ${ibi_uti_amount}",
        "\t\t+ ${ibi_oth_amount};",
        "\tibi_fed = ${ibi_fed_percent}/100 * cost_installed;",
        "\tibi_fed = min(ibi_fed,${ibi_fed_percent_maxvalue});",
        "\tibi_sta = ${ibi_sta_percent}/100 * cost_installed;",
        "\tibi_sta = min(ibi_sta,${ibi_sta_percent_maxvalue});",
        "\tibi_uti = ${ibi_uti_percent}/100 * cost_installed;",
        "\tibi_uti = min(ibi_uti,${ibi_uti_percent_maxvalue});",
        "\tibi_oth = ${ibi_oth_percent}/100 * cost_installed;",
        "\tibi_oth = min(ibi_oth,${ibi_oth_percent_maxvalue});",
        "\tibi = ibi + ibi_fed + ibi_sta + ibi_uti + ibi_oth;\t",
        "\t",
        "\tcapacity = ${system_capacity}*1000; //kW to W",
        "\tcbi_fed = capacity * ${cbi_fed_amount};",
        "\tcbi_fed = min(cbi_fed,${cbi_fed_maxvalue});",
        "\tcbi_sta = capacity * ${cbi_sta_amount};",
        "\tcbi_sta = min(cbi_sta,${cbi_sta_maxvalue});",
        "\tcbi_uti = capacity * ${cbi_uti_amount};",
        "\tcbi_uti = min(cbi_uti,${cbi_uti_maxvalue});",
        "\tcbi_oth = capacity * ${cbi_oth_amount};",
        "\tcbi_oth = min(cbi_oth,${cbi_oth_maxvalue});",
        "\tcbi = cbi_fed + cbi_sta + cbi_uti + cbi_oth;",
        "\t",
        "\tc = cost_installed - ibi - cbi;\t",
        "\treturn c; ",
        "};",
        "",
        "equations{ 'loan_amount' } = define() {",
        "\treturn ${ui_net_capital_cost}*${debt_fraction}/100; };",
        "",
        "equations{ 'ui_wacc' } = define() {",
        "\treturn 100*(",
        "( ",
        "(1+${real_discount_rate}/100)*(1+${inflation_rate}/100)-1)",
        "*",
        "(1-${debt_fraction}/100)",
        "+(1-(${federal_tax_rate}[0]/100",
        "*(1-${state_tax_rate}[0]/100)",
        "+${state_tax_rate}[0]/100))",
        "*${loan_rate}/100",
        "*${debt_fraction}/100); };",
        "",
        "equations{ 'market' } = define() {",
        "  fin = financing();",
        "  if ( fin == 'Residential')",
        "\ta = 0;",
        "  else //  || fin == 'Commercial' uses Financing Min DSCR and market set to 1 by default.",
        "    a = 1;",
        "  return a;",
        "};",
        ""
    ],
    "Callbacks": [
        "on_load{'Financial Debt Residential'} = define() {",
        "\ttxt = debt_messages();",
        "\tproperty('debt_message','caption',txt);",
        "};",
        "",
        "",
        "function debt_messages() {",
        "  msg = '';",
        "  ",
        "  fin = financing();",
        "  debt = true;",
        "  if ( fin == 'All Equity Partnership Flip' || fin == 'Sale Leaseback' ) {",
        "    debt = false;",
        "  }",
        "  if ( debt == true ) {",
        "    if ( fin == 'Residential' || fin == 'Commercial' ) {",
        "      if ( value('loan_term') > value('analysis_period') && value('debt_fraction') > 0 ) {",
        "        msg = msg + 'Debt payment period (loan term) is longer than the analysis period. SAM does not account for debt liability beyond the analysis period. ';",
        "      }  ",
        "    }  ",
        "    else {",
        "      ppa_debt = (value('debt_option') == 1 && value('dscr') > 0) || (value('debt_option')==0 && value('debt_percent')) > 0;",
        "      if ( value('term_tenor') > value('analysis_period') && ppa_debt==true ) {",
        "        msg = msg + 'Debt payment period (tenor) is longer than the analysis period. SAM does not account for debt liability beyond the analysis period. ';",
        "      }  ",
        "    }        ",
        "  }",
        "  return msg;  ",
        "}",
        "",
        "on_change{'loan_term'} = define() {",
        "\ttxt = debt_messages();",
        "\tproperty('debt_message','caption',txt);",
        "};",
