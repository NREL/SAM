    "Callbacks": [
        "on_load{'Financial CS Costs'} = define()",
        "{",
        "\tproperty( 'cs_cost_recurring_fixed','FixedLength',${analysis_period});",
        "\tproperty( 'cs_cost_recurring_capacity','FixedLength',${analysis_period});",
        "\tproperty( 'cs_cost_recurring_generation','FixedLength',${analysis_period});",
        "\tenable_cost_escalation_rate();",
        "};",
        "",
        "on_change{'cs_cost_recurring_fixed'} = define()",
        "{",
        "\tenable('cs_cost_recurring_fixed_escal', #${cs_cost_recurring_fixed} == 1 ); ",
        "};",
        "",
        "on_change{'cs_cost_recurring_capacity'} = define()",
        "{",
        "\tenable('cs_cost_recurring_capacity_escal', #${cs_cost_recurring_capacity} == 1 ); ",
        "};",
        "",
        "on_change{'cs_cost_recurring_generation'} = define()",
        "{",
        "\tenable('cs_cost_recurring_generation_escal', #${cs_cost_recurring_generation} == 1 ); ",
        "};",
        "",
        "function enable_cost_escalation_rate() ",
        "{",
        "\tenable('cs_cost_recurring_fixed_escal', #${cs_cost_recurring_fixed} == 1 ); ",
        "\tenable('cs_cost_recurring_capacity_escal', #${cs_cost_recurring_capacity} == 1 ); ",
        "\tenable('cs_cost_recurring_generation_escal', #${cs_cost_recurring_generation} == 1 ); ",
        "}",
