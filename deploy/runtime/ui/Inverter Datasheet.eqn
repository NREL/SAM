equations{ 'pv.inv.ds.pnt_suggested' } = define() {
	return 0.00025*${pv.inv.ds.paco}; };

equations{ 'pv.inv.ds.pso_suggested' } = define() {
	return ?${pv.inv.ds.eff_type}
[ 0.0, 
0.008*${pv.inv.ds.paco}]; };

equations{ 'pv.inv.ds.eff' } = define() {
	return ?${pv.inv.ds.eff_type}
[ ${pv.inv.ds.eff_weighted},
  ${pv.inv.ds.eff_peak_or_nom} ]; };

equations{ 'pv.inv.ds.pdco' } = define() {
	return ${pv.inv.ds.paco}/(${pv.inv.ds.eff}/100.0); };

