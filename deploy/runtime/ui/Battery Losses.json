    "Equations": [],
    "Callbacks": [
        "on_load{'Battery Losses'} = define()",
        "{",
        "\ttoggle_losses();\r",
        "\tbatt_loss_adjustment_set() ;",
        "};",
        "on_change{'batt_loss_choice'} = define()",
        "{",
        "\ttoggle_losses();",
        "};",
        "function toggle_losses()",
        "{",
        "\tmode = ${batt_loss_choice};",
        "\tmonthly = true;",
        "\tif (mode == 1)",
        "\t\tmonthly = false;",
        "",
        "\tenable('batt_losses_charging', monthly);",
        "\tenable('batt_losses_discharging', monthly);",
        "\tenable('batt_losses_idle', monthly);",
        "\tenable('batt_losses', !monthly);",
        "}\r",
        "\r",
        "function batt_loss_adjustment_set() \r",
        "{\r",
        "\tif (financing() != \"LCOE Calculator\" && financing() != \"None\") {\r",
        "\t\tif (value('system_use_lifetime_output') == 1) {\r",
        "\t\t\tproperty('batt_adjust', 'AnalysisPeriod', value('analysis_period'));\r",
        "\t\t}\r",
        "\t\telse {\r",
        "\t\t\tproperty('batt_adjust', 'AnalysisPeriod', 1);\r",
        "\t\t}\r",
        "\t}\r",
        "\telse {\r",
        "\t\tproperty('batt_adjust', 'AnalysisPeriod', 1);\r",
        "\t}\r",
        "}",
        ""
    ]
