        "\t\t\tCap_low = input[idx-1][0];\r",
        "\t\t\tCap_high = input[idx][0];\t\r",
        "\t\t\tcost_low = input[idx-1][1];\r",
        "\t\t\tcost_high = input[idx][1];\r",
