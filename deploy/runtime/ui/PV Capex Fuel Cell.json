    "Equations": [
        "equations{'fuelcell_power_kw'} = define() {",
        "\treturn ${fuelcell_power_total};",
        "};\r",
        "\r",
        "equations{'$MIMO$ interpolate_fuelcell_capex_table'} = define()\r",
        "{\r",
        "\tinput = ${capex_table_fuelcell_inputs};\r",
        "\t\r",
        "\tsys_cap = ${fuelcell_power_kw};\r",
        "\t\r",
        "\tnr = #input;\r",
        "\tnc = #input[0];\r",
        "\t//refresh();\r",
        "\tidx=0;\r",
        "\t//Cap_low = [];\r",
        "\t//Cap_high = [];\r",
        "\tif (nc == 1) {\r",
        "\t\tcost = 0;\r",
        "\t\ttotal_cost = cost*sys_cap;\r",
        "\t\t${capex_table_fuelcell_cost_per_kw} = cost;\r",
        "\t\t${capex_table_total_installed_fuelcell_cost} = total_cost;\r",
        "\t\texit;\r",
        "\t}\r",
        "\t//property('capex_table_inputs','ShowColumn','Capacity (kW),Costs ($/kW)');\r",
        "\tif (sys_cap > input[nr - 1][0]) {\r",
        "\t\tcost = input[nr - 1][1];\r",
        "\t\t\t\r",
        "\t}\r",
        "\telse if (sys_cap < input[0][0]) {\r",
        "\t\tcost = input[0][1];\r",
        "\t\t\t\r",
        "\t}\r",
        "\telse {\r",
