        "\t\t\tcost_low = input[0][1];\r",
        "\t\t\tcost_high = input[1][1];\r",
        "\t\t}\r",
        "\t\tcost = (sys_cap - Cap_low)/(Cap_high - Cap_low) * (cost_high - cost_low) + cost_low;\r",
