    "VarDatabase": {
        "reopt_consider_outage": {
            "Version": 4.0,
            "Type": 1.0,
            "Label": "Consider grid outage in REopt optimization",
            "Units": "",
            "Group": "Battery REopt Optimization",
            "IndexLabels": "",
