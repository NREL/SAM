    "Callbacks": {
        "1": "// This form is a copy of Financial Solution Flip Leaseback",
        "2": "// so ppa_soln_mode radio button can have different labels",
        "3": "// to indicate IRR target is for the tax investor for flip ",
        "4": "// structures. Any updates should be made to both forms.",
        "5": "",
        "6": "on_load{'Financial Solution Mode Single Owner'} = define()",
        "7": "{",
        "8": "\tsoln_mode_ctrl_refresh();",
        "9": "\tif (is_assigned('en_batt') && value('en_batt') != 0 && financing() != \"Host Developer\") {",
        "10": "\t\tcheck_auto_ppa();",
        "11": "\t}",
        "12": "\tdisplay_message();",
        "13": "\ton_change{'ppa_soln_mode'};",
        "14": "};",
        "15": "",
        "16": "on_change{'ppa_soln_mode'} = define()",
        "17": "{",
        "18": "\tsoln_mode_ctrl_refresh();",
        "19": "\tif (is_assigned('en_batt') && value('en_batt') != 0 && financing() != \"Host Developer\") {",
        "20": "\t\tcheck_auto_ppa();",
        "21": "\t}",
        "22": "\tdisplay_message();",
        "23": "};",
        "24": "",
        "25": "on_change{'ppa_price_input'} = define()",
        "26": "{",
        "27": "\tsoln_mode_ctrl_refresh();",
        "28": "};",
        "29": "",
        "30": "on_change{'flip_target_year'} = define () {",
        "31": "\tdisplay_message();",
        "32": "};",
        "33": "",
        "34": "function display_message() ",
        "35": "{",
        "36": "\ttxt = '';",
        "37": "\tclr = 'black';",
        "38": "\tif ( value('flip_target_year') > value('analysis_period') )",
        "39": "\t{",
        "40": "\t\ttxt += 'IRR target year is greater than the analysis period. ';",
        "41": "\t\tclr = 'red';",
        "42": "\t}",
        "43": "\tif ( financing() == 'Host Developer' ) ",
        "44": "\t{",
        "45": "\t\ttxt += 'IRR is the developer internal rate of return. ';",
        "46": "\t}",
        "47": "\telseif( financing() == 'Community Solar' )",
        "48": "\t{",
        "49": "\t\ttxt += 'Set PPA price or IRR target to zero if project only earns revenue from subscription payments on the Community Solar page.';",
        "50": "\t}",
        "51": "\telseif ( value('ppa_soln_mode') == 0 && value('en_electricity_rates') == 0 )",
        "52": "\t{",
        "53": "\t\tclr = 'red';",
        "54": "\t\ttxt += 'The Specify IRR Target option is not available with the Use PPA Price option on Electricity Purchases page. Either change the option on the Electricity Purchases page or use the Specify PPA Price option.';",
        "55": "\t}",
        "56": "\tproperty('solution_mode_message','Caption',txt);",
        "57": "\tproperty('solution_mode_message','TextColour',clr);",
        "58": "}",
        "59": "",
        "60": "function soln_mode_ctrl_refresh()",
        "61": "{",
        "62": "    is_specify_irr = ( value('ppa_soln_mode') == 0 );",
        "63": "    enable( 'flip_target_percent', is_specify_irr );",
        "64": "    enable( 'ppa_price_input', !is_specify_irr );",
        "65": "\tshow_escalation = (#${ppa_price_input} < 2);",
        "66": "\tenable(\"ppa_escalation\",(show_escalation || is_specify_irr));",
        "67": "}"
    }
