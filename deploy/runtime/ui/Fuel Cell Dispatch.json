    "Equations": [
        "equations{ 'fuelcell_dispatch'} = define(){",
        "\tdispatch = ${fuelcell_dispatch_input};",
        "\tif (${fuelcell_dispatch_input_units} == 1){",
        "\t\tfor (i = 0; i < #dispatch; i++){",
        "\t\t\tdispatch[i] = dispatch[i] * ${fuelcell_unit_max_power};",
        "\t\t}",
        "\t}",
        "\treturn dispatch;",
        "};"
    ],
