        "on_change{'fuelcell_dispatch_choice'} = define()",
        "{",
        "\tenable_fc_dispatch_options();",
        "\tvalue('fuelcell_dispatch_excl', value('fuelcell_dispatch_choice'));\r",
        "};",
        "",
        "function enable_fc_dispatch_options()",
        "{",
        "\tenable('fuelcell_fixed_pct', value('fuelcell_dispatch_choice') == 0);",
        "\tenable('fuelcell_dispatch_input', value('fuelcell_dispatch_choice') == 3);",
        "\tenable('fuelcell_dispatch_input_units', value('fuelcell_dispatch_choice') == 3);",
