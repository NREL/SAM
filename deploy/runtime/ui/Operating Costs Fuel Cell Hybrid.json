    "Equations": [
        "",
        "equations{ 'om_fuel_cost'} = define(){",
        "\tfuel_price = ${fc_fuel_cost};",
        "\t// need all prices in $/MMBtu ",
        "\t// Convert $/MCF to $/MMBtu (1 MCF = 1.037 MCF)",
        "\tif (${om_fuel_price_units} == 0)",
        "\t{",
        "\t\tfor(i=0;i<#fuel_price;i++)",
        "\t\t{",
        "\t\t\tfuel_price[i] = fuel_price[i] * 1.037;",
        "\t\t}\t",
        "\t}",
        "\treturn fuel_price;",
        "};",
        "",
        "/*",
        "Set additional o and m cost values calculated in financial models",
        "only one fuel cost ",
        "Capacity and production o and m costs cannot be combined ",
        "since have different multipliers base on technology",
        "*/",
        "equations{ 'add_om_num_types'} = define(){",
        "\treturn 2;",
        "};",
