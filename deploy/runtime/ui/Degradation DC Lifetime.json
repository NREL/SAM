        "\t{",
        "\t\tdisplayflag = true;",
        "\t}\t",
        "\tshow('degrad_sched_warning', displayflag);",
        "\trefresh();",
        "}",
        "",
        "on_change{'dc_degradation'} = define()",
        "{",
