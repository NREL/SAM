        "",
        "// Just combo converter",
        "equations{ 'CT' } = define() {",
        "\treturn ${csp.pt.pwrb.condenser_type} + 1; };",
        "",
        "// Just combo converter",
        "equations{ 'tech_type' } = define() {",
        "\tif (${csp.pt.pwrb.pressure_mode} == 1) //sliding pressure",
        "\t\treturn 3; // user defined",
        "\telseif(${csp.pt.pwrb.pressure_mode}==2) //first custom cycle",
        "\t\treturn 5;",
        "\telseif(${csp.pt.pwrb.pressure_mode}==3) //second custom cycle",
        "\t\treturn 6;",
        "\telse // fixed pressure",
        "\t\treturn 1; // tower",
        "};",
        "",
        ""
    ],
    "Callbacks": [
        "on_load{\"Rankine Cycle Air or Evaporative Cooling\"} = define() {",
        "\ton_change{\"csp.pt.pwrb.condenser_type\"}();",
        "\ton_change{\"T_amb_des\"};",
        "\ton_change{\"P_cond_min\"};",
        "};",
        "",
        "on_change{\"csp.pt.pwrb.condenser_type\"} = define() {",
        "\tmode = value(\"csp.pt.pwrb.condenser_type\");",
        "\t",
        "\tshow(\"label_hybrid_cooling\", mode==2);",
        "\tshow(\"acc_pressure_plot\", mode==1);",
        "\tupdate_acc_press_plot();",
        "\t",
        "\t// 0 = evap, 1 = air, 2 = hybrid, 3=radiative",
        "\tenable(\"T_approach\", mode==0||mode==2||mode==3);",
        "\tenable(\"dT_cw_ref\", mode==0||mode==2);",
        "\tenable(\"T_ITD_des\", mode==1||mode==2);",
        "\tenable(\"P_cond_ratio\", mode==1||mode==2);",
        "",
        "\tif (mode==3) {",
        "\t\tproperty(\"cycle_technology_group_box\", \"Height\", 798);",
        "\t\tproperty(\"radiative_cooling_divider\", \"Y\", 309);",
        "\t\tproperty(\"radiator_field_divider\", \"Y\", 336);",
        "\t\tproperty(\"cold_storage_divider\", \"Y\", 336);",
        "\t\tproperty(\"cost_divider\", \"Y\", 666);",
        "\t\tproperty(\"csp.pt.pwrb.pressure_mode\", \"Items\",",
        "\t\t [\"Fixed pressure\",",
        "\t\t \"Sliding pressure\",",
        "\t\t \"Sliding for high backpressure\",",
        "\t\t \"Sliding for low backpressure\"]);",
        "\t}",
        "\telse {",
        "\t\tproperty(\"cycle_technology_group_box\", \"Height\", 300);",
        "\t\tproperty(\"radiative_cooling_divider\", \"Y\", -50);",
        "\t\tproperty(\"radiator_field_divider\", \"Y\", -50);",
        "\t\tproperty(\"cold_storage_divider\", \"Y\", -50);",
        "\t\tproperty(\"cost_divider\", \"Y\", -50);",
        "\t\tif (value(\"csp.pt.pwrb.pressure_mode\") > 1)",
        "\t\t\t// keep \"Turbine inlet pressure control\" from being blank",
        "\t\t\tvalue(\"csp.pt.pwrb.pressure_mode\", 0);",
        "\t\tproperty(\"csp.pt.pwrb.pressure_mode\", \"Items\",",
        "\t\t [\"Fixed pressure\",",
        "\t\t \"Sliding pressure\"]);",
        "\t}",
        "",
        "\trefresh();",
        "};",
        "on_change{\"T_amb_des\"} = define(){update_acc_press_plot();};",
        "on_change{\"P_cond_min\"} = define(){update_acc_press_plot();};",
        "",
        "function update_acc_press_plot()",
        "{",
        "\ttable = acc_press_vs_load_temp();",
        "\tdesPcond = table{'desPcond'};",
        "\tp_cond = table{'p_cond'};",
        "\tTamb = table{'Tamb'};",
        "\t",
        "\t",
        "\tclearplot( 'acc_pressure_plot');",
        "\tplot( Tamb, p_cond, {'thick'=3, 'type' = 'line', 'color' = 'blue'});//, 'series' = 'Tamb: ' + ${T_amb_des} + ' [C]'",
        "\tplotopt( {'title' = 'Air-cooled Condenser Model (full-load)', 'legend' = true});//, 'legendpos'=[15,12]",
        "\taxis( 'x1', {'label' = 'Ambient Temperature [C]', 'min' = 0, 'max' = ${T_amb_des}*1.1});",
        "\taxis( 'y1', {'label' = 'Condenser Pressure [inHg]', 'min'= 0, 'max' = desPcond*1.1});",
        "}",
        "",
        "function acc_press_vs_load_temp()",
        "{",
        "\tP_cond_min = ${P_cond_min};",
        "\tT_amb_des = ${T_amb_des};",
        "\t",
        "\t// Fitted regression model coefficients",
        "\tA = [[147.96619, 71.23482281, 27.55395547],",
        "\t\t [-329.021562, -159.2675368, -62.24857193],",
        "\t\t [183.4601872, 89.50235831, 35.57127305]];",
        "\t",
        "\tNsteps = 10;",
        "\t",
        "\t// Normalized factors ",
        "\tp_norm = 0.;",
        "\tT_norm = 1.0;\t// normalized ambient temperature",
        "\tq_norm = 1.; \t// Approximately Full load",
        "\tt_step = (T_norm - 0.85)/Nsteps;",
        "\tfor (t = 0; t <= Nsteps + 1; t++){",
        "\t\tif (T_norm > 0.8925){",
        "\t\t\tfor (i = 0; i < #A; i++){",
        "\t\t\t\tfor (j = 0; j < #A[i]; j++){",
        "\t\t\t\t\tp_norm += A[i][j]*pow(T_norm, i)*pow(q_norm,j);",
        "\t\t\t\t}",
        "\t\t\t}",
        "\t\t}",
        "\t\telse",
        "\t\t\tp_norm = 1;",
        "\t",
        "\t\tT_vec[t] = T_norm*(T_amb_des+273.15) - 273.15;",
        "\t\tif (T_norm == 1.0)",
        "\t\t\tdesPcond = p_norm*P_cond_min;",
        "\t\t\t",
        "\t\tif (p_norm > 1)",
        "\t\t\tp_vec[t] = p_norm*P_cond_min;",
        "\t\telse",
        "\t\t\tp_vec[t] = P_cond_min;",
        "\t\t// step down",
        "\t\tT_norm -= t_step;",
        "\t\tp_norm = 0.0; // Reset p",
        "\t}",
        "\t",
        "\ttable{'desPcond'} = desPcond;",
        "\ttable{'Tamb'} = T_vec;",
        "\ttable{'p_cond'} = p_vec;",
        "\treturn table;",
        "}",
        "",
        "",
        "",
        ""
    ]
