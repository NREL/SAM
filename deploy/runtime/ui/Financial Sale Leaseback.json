    ],
    "Callbacks": [
        "on_load{'Financial Analysis Parameters'} = define() {",
        "\ton_change{'tax_investor_required_lease_reserve'}();",
        "\ttxt = sale_leaseback_messages();",
        "\tproperty('sale_leaseback_message','caption',txt);",
        "};",
        "",
        "function sale_leaseback_messages() {",
        "\tmsg = '';",
        "\tif ( value('tax_investor_required_lease_reserve')/12 > value('analysis_period') )",
        "\t\tmsg = 'Lease payment reserve period is greater than analysis period. ';",
        "\tif ( value('tax_investor_required_lease_reserve')/12 > value('flip_target_year') )",
        "\t\tmsg = msg + 'Lease payment reserve period is greater than IRR target year. ';",
        "}",
        "",
        "on_change{'tax_investor_required_lease_reserve'} = define(){",
        "\ttxt = sale_leaseback_messages();",
        "\tproperty('sale_leaseback_message','caption',txt);",
        "};",
        ""
    ]
