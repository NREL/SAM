    "Callbacks": [
        "on_load{'Battery Dispatch Standalone Options BTM'} = define() {",
        "\tshow_hide_charge_options();\r",
        "\tptc_pbi_with_grid_charging_message();",
        "};",
        "",
        "on_change{'batt_dispatch_choice_ui'} = define() {",
        "\tshow_hide_charge_options();\r",
        "\tptc_pbi_with_grid_charging_message();\t",
        "\tvalue('batt_dispatch_excl', value('batt_dispatch_choice_ui'));",
        "};",
        "\r",
        "on_change{'batt_dispatch_auto_can_gridcharge'} = define() {ptc_pbi_with_grid_charging_message();};\r",
        "\r",
        "// hide charge options for manual dispatch",
        "function show_hide_charge_options()",
        "{",
        "\toption = value('batt_dispatch_choice_ui');",
        "\tif (option == 3) ",
        "\t{",
        "\t\tenable('batt_dispatch_auto_can_gridcharge', false);",
        "\t\tenable('batt_dispatch_auto_btm_can_discharge_to_grid', false);",
        "\t\tshow('batt_dispatch_auto_can_gridcharge', false);",
        "\t\tshow('batt_dispatch_auto_btm_can_discharge_to_grid', false);",
        "\t\trefresh();",
        "\t}",
        "\telse if (option == 2)",
        "\t{",
        "\t\tvalue('batt_dispatch_auto_can_gridcharge', 1);",
        "\t\tenable('batt_dispatch_auto_can_gridcharge', false);",
        "\t\tenable('batt_dispatch_auto_btm_can_discharge_to_grid', true);",
        "\t}",
        "\telse ",
        "\t{",
        "\t\tvalue('batt_dispatch_auto_can_gridcharge', 1);",
        "\t\tvalue('batt_dispatch_auto_btm_can_discharge_to_grid', 0);",
        "\t\tenable('batt_dispatch_auto_can_gridcharge', false);",
        "\t\tenable('batt_dispatch_auto_btm_can_discharge_to_grid', false);",
        "\t}",
        "}",
