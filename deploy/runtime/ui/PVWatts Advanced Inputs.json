    "Equations": [
        "equations{'bifaciality'} = define() {",
        "\tif (${module_is_bifacial}) return 0.7;",
        "\telse return 0;",
        "};",
        "",
        "equations{'albedo'} = define() {",
        "\t// pvwattsv8 accepts single value, monthly array",
        "\t// or time series array for albedo value",
        "\t// set to monthly array so if user changes model",
        "\t// albedo is valid for detailed pv model",
        "\talb = [];",
        "\tfor ( i=0; i<12; i++ )",
        "\t{",
        "\t\talb[i] = ${user_specified_constant_albedo};",
        "\t}\t",
        "\treturn alb;",
        "};",
        "",
        ""
    ],
    "Callbacks": [
        "",
        "on_load{'PVWatts Advanced Inputs'} = define() {",
        "\tsnow_loss_checks();",
        "\talbedo_check();",
        "\tloss_adjustment_set();",
        "};",
        "",
        "on_change{'en_snowloss'} = define() {",
        "\tsnow_loss_checks();\t",
        "};",
        "",
        "on_change{'loss_snow'} = define() {",
        "\tsnow_loss_checks();",
        "};",
        "",
        "on_change{'user_specified_constant_albedo'} = define() {",
        "\talbedo_check();",
        "};",
        "",
        "on_change{'use_wf_albedo'} = define() {",
        "\talbedo_check();",
        "};",
        "",
        "on_change{'edit_3d_scene'} = define() {",
        "\tminute_step = 60;",
        "\tif (varinfo(\"wf_nrecords\"))",
        "\t{",
        "\t\tnrec = value(\"wf_nrecords\");",
        "\t\tif (!isnan(nrec) && (nrec > 0))",
        "\t\t{",
        "\t\t\tminute_step = 60 * 8760 / nrec;",
        "\t\t}",
        "\t}",
        "",
        "\tx = editscene3d( 'shading_3d_scene', ",
        "\t\tvalue('lat'), value('lon'), value('tz'), ",
        "\t\tvalue('city') + ', ' + value('state')  , minute_step);",
        "// update 60 to timestep from weather file.\t",
        "\tif ( x.ierr == 0 )",
        "\t{\t",
        "\t\ttimestep = x.losses;",
        "//\t\tmsgbox(\"timestep = \" + to_string(timestep));",
        "//\t\tmsgbox(\"x = \" + to_string(x));",
        "\t\tif (yesno('Update timeseries and diffuse shading losses with calculated values from 3D scene?'))",
        "\t\t{",
        "\t\t\tshad = value('shading');",
        "\t\t\tshad.en_timestep = true;",
        "\t\t\tshad.timestep = timestep;",
        "\t\t\t// diffuse",
        "\t\t\tshad.en_diff = true;",
        "\t\t\tshad.diff = x.diffuse[0];",
        "",
        "\t\t\tvalue('shading', shad );\t",
        "\t\t}",
        "\t}",
        "\telse",
        "\t{",
        "\t\tmsgbox('Internal error: ' + x.message);",
        "\t}",
        "};",
        "",
        "function snow_loss_checks () {",
        "\tmsg_snow = '';",
        "\tif ( isnan(value('annual_snow')) == true && value('en_snowloss') == 1 ) ",
        "\t{",
        "\t\tmsg_snow = 'Weather file does not have snow depth data required to model snow losses. See Help for details.';",
        "\t}",
        "",
        "\tif ( isnan(value('annual_snow')) == false && value('annual_snow') > 0 && value('en_snowloss') == 0 ) ",
        "\t{",
        "\t\tmsg_snow = 'Weather file has snow depth data. You may want to consider enabling Estimate Snow Losses to model the effect of snow covering the array. See Help for details.';",
        "\t}\t",
        "\tproperty('msg_pvwatts_snow','Caption',msg_snow);",
        "\t",
        "\tif ( value('en_snowloss') == 1 && ",
        "\t     value('loss_snow') !=0 && ",
        "\t     isnan(value('annual_snow')) == false && ",
        "\t     value('en_user_spec_losses') == 0 )",
        "\t{",
        "\t\tmsgbox('Snow loss warning!\\nThe snow loss model is enabled and the Snow input is set to ' + value('loss_snow') + '%. Set Snow to zero to avoid double-counting snow losses. Ignore this message to include losses from both the snow model and the Snow input value.');",
        "\t}",
        "}",
        "",
        "\t",
        "function albedo_check() {",
        "\tenable('user_specified_constant_albedo',!value('use_wf_albedo'));",
        "\tmsg_albedo = '';",
        "\talb = value('user_specified_constant_albedo');",
        "\tif ( (alb <= 0 || alb >= 1) && !value('use_wf_albedo') )",
        "\t{",
        "\t\tmsg_albedo = 'Albedo must be greater than 0 and less than 1. PVWatts uses default albedo for time steps with invalid albedo. See Help for details.';",
        "\t}",
        "\tproperty('msg_pvwatts_albedo','Caption',msg_albedo);",
        "",
        "}",
        "",
        "function loss_adjustment_set() ",
        "{",
        "\tif (financing() != \"LCOE Calculator\" && financing() != \"None\") {",
        "\t\tif (value('system_use_lifetime_output') == 1) {",
        "\t\t\tproperty('adjust', 'AnalysisPeriod', value('analysis_period'));",
        "\t\t}",
        "\t\telse {",
        "\t\t\tproperty('adjust', 'AnalysisPeriod', 1);",
        "\t\t}",
        "\t}",
        "\telse {",
        "\t\tproperty('adjust', 'AnalysisPeriod', 1);",
        "\t}",
        "}"
    ]
}