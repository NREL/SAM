    "Equations": [
        "equations{ '$MIMO$ dacdcld_analysis_period' } = define() {",
        "\tif ((analysis_period_old()>= 1) && (${analysis_period} != analysis_period_old())) {",
        "\t\taclosses = alloc(${analysis_period}*365); // daily",
        "\t\tfor(i=0; i<#aclosses && i <#${ac_lifetime_losses}; i++)",
        "\t\t\taclosses[i] = ${ac_lifetime_losses}[i];",
        "\t\t${ac_lifetime_losses_ui} = aclosses;",
        "\t\tdclosses = alloc(${analysis_period}*365); // daily",
        "\t\tfor(i=0; i<#aclosses && i <#${dc_lifetime_losses}; i++)",
        "\t\t\tdclosses[i] = ${dc_lifetime_losses}[i];",
        "\t\t${dc_lifetime_losses_ui} = dclosses;",
        "\t}",
        "};",
        "\t\t"
    ],
    "Callbacks": [
        "function enable_lifetime_losses()",
        "{",
        "\tenable('dc_lifetime_losses', value('en_dc_lifetime_losses'));",
        "\tenable('ac_lifetime_losses', value('en_ac_lifetime_losses'));",
        "}",
        "",
        "on_load{'Degradation AC DC Lifetime Daily'} = define() {",
        "\tenable_lifetime_losses();",
        "\tproperty('dc_lifetime_losses', 'Description','Daily DC loss over project life.\\nAnalysis period on Financial Parameters page.\\n  Number of Rows = \\n  1 row/day * 365 days * ' + value('analysis_period') + ' years');",
        "\tproperty('dc_lifetime_losses', 'Mode', 2); // daily",
        "\tproperty('ac_lifetime_losses', 'Description','Daily AC loss over project life.\\nAnalysis period on Financial Parameters page.\\n  Number of Rows = \\n  1 row /day * 365 days * ' + value('analysis_period') + ' years');",
        "\tproperty('ac_lifetime_losses', 'Mode', 2); // daily",
        "};",
        "",
        "on_change{'en_ac_lifetime_losses'} = define() {",
        "\tenable_lifetime_losses();",
        "};",
        "",
        "on_change{'en_dc_lifetime_losses'} = define() {",
        "\tenable_lifetime_losses();",
        "};",
        "",
