    "Equations": [
        "equations{'forecast_price_signal_model'} = define()",
        "{",
        "\tif (financing() == 'Merchant Plant')",
        "\t\treturn 1;",
        "\telse",
        "\t\treturn 0;",
        "};",
        "/*",
        "equations{ '$MIMO$ mp_analysis_period_changed' } = define() {",
        "\t${mp_energy_market_revenue} = ${mp_energy_market_revenue_ui};",
        "\treturn 0;",
        "};",
        "*/",
        "",
        "equations{ '$MIMO$ mp_check_cleared_capacity' } = define() {",
        "\tdata.analysis_period =  ${analysis_period};",
        "\tdata.system_capacity =  ${system_capacity};",
        "\tdata.mp_enable_energy_market_revenue =  ${mp_enable_energy_market_revenue};",
        "\tdata.mp_enable_ancserv1 =  ${mp_enable_ancserv1};",
        "\tdata.mp_enable_ancserv2 =  ${mp_enable_ancserv2};",
        "\tdata.mp_enable_ancserv3 =  ${mp_enable_ancserv3};",
        "\tdata.mp_enable_ancserv4 =  ${mp_enable_ancserv4};",
        "\tdata.mp_energy_market_revenue =  ${mp_energy_market_revenue};",
        "\tdata.mp_ancserv1_revenue =  ${mp_ancserv1_revenue};",
        "\tdata.mp_ancserv2_revenue =  ${mp_ancserv2_revenue};",
        "\tdata.mp_ancserv3_revenue =  ${mp_ancserv3_revenue};",
        "\tdata.mp_ancserv4_revenue =  ${mp_ancserv4_revenue};",
        "\tdata.mp_energy_market_revenue_single =  ${mp_energy_market_revenue};",
        "\tdata.mp_ancserv1_revenue_single =  ${mp_ancserv1_revenue_single};",
        "\tdata.mp_ancserv2_revenue_single =  ${mp_ancserv2_revenue_single};",
        "\tdata.mp_ancserv3_revenue_single =  ${mp_ancserv3_revenue_single};",
        "\tdata.mp_ancserv4_revenue_single =  ${mp_ancserv4_revenue_single};",
        "",
        "\tsuccess = ssc_eqn('mp_ancillary_services', data);",
        "\t",
        "\tif (!success){",
        "\t\tmsgbox('success ' + success);",
        "\t\treturn 0;",
        "\t}",
        "\t",
        "\tif (data.mp_capacity_check == 0)",
        "\t{",
        "\t\tmsg = data.mp_capacity_check_error;",
        "\t\t${mp_error_msg} = msg;",
        "\t\tmsgbox(msg);",
        "\t}",
        "\treturn 0;",
        "};",
        ""
    ],
    "Callbacks": [
        "function update_merchant_plant_inputs()",
        "{",
        "\tmarket_enable = value('mp_enable_energy_market_revenue');",
        "\tenable('mp_energy_market_revenue', market_enable && !value('mp_enable_market_percent_gen'));",
        "\tenable('mp_energy_market_revenue_single', market_enable && value('mp_enable_market_percent_gen'));",
        "\tenable('mp_enable_market_percent_gen', market_enable);",
        "\tenable('mp_market_percent_gen',market_enable && value('mp_enable_market_percent_gen'));",
        "}",
        "",
        "on_load{'Financial Merchant Plant Energy Market Revenue'} = define() {",
        "\tupdate_merchant_plant_inputs();",
        "};",
        "",
        "on_change{'mp_enable_energy_market_revenue'} = define() {",
        "\tupdate_merchant_plant_inputs();",
        "};",
        "",
        "on_change{'mp_enable_market_percent_gen'} = define() {",
        "\tupdate_merchant_plant_inputs();",
        "};",
        ""
    ]
