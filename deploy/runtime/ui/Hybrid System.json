    "Equations": [
        "// ui_pv_capacity is used on operating costs form, so use ui_hyb_pv_capacity here\t\t\r",
        "equations{ 'ui_hyb_pv_capacity' } = define() {\r",
        "\tif ( technology() == 'Photovoltaic Wind Battery Hybrid' ) {\r",
        "\t\treturn ${total_inverter_capacity};\r",
        "\t}\r",
        "\telse {\r",
        "\t\treturn ${ac_nameplate};\r",
        "\t}\t\r",
        "};\r",
        "\r",
        "equations{ 'ui_battery_capacity' } = define() {\r",
        "\treturn ${batt_power_discharge_max_kwac};\r",
        "};\r",
        "\r",
        "equations { 'ui_fuel_cell_capacity' } = define() { \r",
        "\tif ( technology() == 'PVWatts Wind FuelCell Battery Hybrid' || technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' ) {\r",
        "\t\treturn ${fuelcell_system_capacity};\r",
        "\t}\r",
        "\telse {\r",
        "\t\treturn 0;\r",
        "\t}\r",
        "};\r",
        "\r",
        "equations { 'ui_customgeneration_capacity' } = define() {\r",
        "\tif ( technology () == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' ) {\r",
        "\t\treturn ${customgeneration_system_capacity};\r",
        "\t}\r",
        "\telse {\r",
        "\t\treturn 0;\r",
        "\t}\r",
        "};\r",
        "\r",
        "equations{ 'system_capacity' } = define() {\r",
        "\tif ( technology() == 'PVWatts Wind Battery Hybrid') {\r",
        "\t\treturn ${ac_nameplate} \r",
        "\t\t+ ${wind_system_capacity}; \r",
        "\t}\r",
        "\telseif ( technology() == 'PVWatts Wind FuelCell Battery Hybrid' ) { \r",
        "\t\treturn ${ac_nameplate}\r",
        "\t\t+ ${wind_system_capacity} \r",
        "\t\t+ ${fuelcell_system_capacity}; \r",
        "\t}\r",
        "\telseif ( technology() == 'Photovoltaic Wind Battery Hybrid' ) {\r",
        "\t\treturn ${total_inverter_capacity}\r",
        "\t\t+ ${wind_system_capacity}; \r",
        "\t}\r",
        "\telseif ( technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' ) {\r",
        "\t\treturn ${customgeneration_system_capacity} \r",
        "\t\t+ ${ac_nameplate}\r",
        "\t\t+ ${wind_system_capacity}\r",
        "\t\t+ ${fuelcell_system_capacity}; \r",
        "\t}\r",
        "\telse {\r",
        "\t\treturn null;\r",
        "\t}\r",
        "};\r",
        ""
    ],
    "Callbacks": [
        "on_load{'Hybrid System'} = define() {\r",
        "\tmsg = '';\r",
        "\tif ( technology() == 'PVWatts Wind Battery Hybrid' )\r",
        "\t{\r",
        "\t\tmsg = 'The hybrid system combines generation profiles from the PVWatts and Wind models. This electricity is available to charge the battery, meet the electric load, or be delivered to the grid, depending on your application.';\r",
        "\t}\r",
        "\telseif ( technology() == 'PVWatts Wind FuelCell Battery Hybrid' )\r",
        "\t{\r",
        "\t\tmsg = 'The hybrid system combines generation profiles from the PVWatts, Wind, and Fuel Cell models. This electricity is available to charge the battery, meet the electric load, or be delivered to the grid, depending on your application.';\r",
        "\t}\r",
        "\telseif ( technology() == 'Photovoltaic Wind Battery Hybrid' )\r",
        "\t{\r",
        "\t\tmsg = 'The hybrid system combines generation profiles from the Detailed PV and Wind models. This electricity is available to charge the battery, meet the electric load, or be delivered to the grid, depending on your application.';\r",
        "\t}\r",
        "\telseif ( technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' )\r",
        "\t{\r",
        "\t\tmsg = 'The hybrid system combines generation profiles from the Custom Generation Profile, PVWatts, Wind, and Fuel Cell models. This electricity is available to charge the battery, meet the electric load, or be delivered to the grid, depending on your application.';\r",
        "\t}\r",
        "\telse\r",
        "\t{\r",
        "\t\tmsg = 'Unrecognized hybrid system configuration: ' + technology();\r",
        "\t}\r",
        "\tmsg = msg + ' The capacity of each subsystem is defined on its system design page.';\r",
        "\tproperty('lbl_hybrid_description','Caption',msg);\r",
        "\tshow( 'ui_customgeneration_capacity', technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' );\r",
        "\tshow( 'ui_fuel_cell_capacity', ( technology() == 'PVWatts Wind FuelCell Battery Hybrid' || technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' ) );\r",
        "};\r",
        ""
    ]
}