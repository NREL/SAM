    "Callbacks": [
        "on_load{'Battery Dispatch Automated FOM'}=define()",
        "{\t",
        "\t// fom = front of meter",
        "\tvalue('batt_dispatch_choice', 0);",
        "\ttoggle_dispatch_fom_automated();",
        "\tshow_hide_charge_options();",
        "\tproduction_forecast_msg_auto();",
        "\tinvalid_option_message();",
        "\trefresh();",
        "};",
        "",
        "on_change{'batt_dispatch_wf_forecast_choice'}=define()",
        "{ ",
        "\ttoggle_dispatch_fom_automated();\r",
        "\tproduction_forecast_msg_auto();",
        "\tinvalid_option_message();",
        "};",
        "",
        "on_change{'batt_select_weather_file_forecast'}=define()",
        "{",
        "\tfile = choose_file( \"\", 'Choose a weather file', ",
        "\t\t\t'All Weather Files (*.tm2,*.tm3,*.epw,*.smw,*.csv)|*.tm2;*.tm3;*.epw;*.smw;*.csv');",
        "\tvalue('batt_user_specified_weather_file', file);",
        "\trun_forecast(file);",
        "};",
        "",
        "on_change{'ui_batt_is_dc'}=define()",
        "{",
        "\tenable_charge_from_clip();",
        "};",
        "",
        "on_change{'batt_cycle_cost_choice'}=define()",
        "{",
        "\tenable('batt_cycle_cost', value('batt_cycle_cost_choice') == 1);",
        "};",
        "",
        "on_change{'batt_pv_ac_forecast_fom_auto'}=define()",
        "{",
        "\trun_generic_forecast(value('batt_pv_ac_forecast_fom_auto'));",
        "};",
        "",
        "function toggle_dispatch_fom_automated()",
        "{",
        "",
        "\t// Turn on/off forecast input",
        "\t//toggle_forecast();",
        "\tenable('batt_select_weather_file_forecast', value('batt_dispatch_wf_forecast_choice') == 2);",
        "\tenable('batt_user_specified_weather_file', value('batt_dispatch_wf_forecast_choice') == 2);",
        "\tenable('batt_pv_ac_forecast_fom_auto', value('batt_dispatch_wf_forecast_choice') == 2);",
        "\t",
        "\tif (value('batt_dispatch_wf_forecast_choice') == 2)",
        "\t{",
        "\t\tac_forecast = value(\"batt_pv_ac_forecast\");",
        "\t\tclipping_forecast = value(\"batt_pv_clipping_forecast\");",
        "\t\tforecast_array = value(\"batt_pv_ac_forecast_fom_auto\");",
        "\t\tif (#ac_forecast <= 1 || #clipping_forecast <= 1) { ",
        "\t\t\tif (technology() == 'PV Battery') {",
        "\t\t\t\tfile = value('batt_user_specified_weather_file');",
        "\t\t\t\tif (file != \"\") {",
        "\t\t\t\t\trun_forecast(file);",
        "\t\t\t\t}",
        "\t\t\t}",
        "\t\t\telse {",
        "\t\t\t\t// Generic system",
        "\t\t\t    if (#forecast_array > 1) {",
        "\t\t\t\t\trun_generic_forecast(forecast_array);",
        "\t\t\t\t}",
        "\t\t\t}",
        "\t\t}",
        "\t}",
        "\telse {",
        "\t\tvalue('batt_pv_ac_forecast', [0]);",
        "\t\tvalue('batt_pv_clipping_forecast', [0]);",
        "\t}",
        "",
        "\tmode = value('batt_dispatch_wf_forecast_choice');",
        "\t// automated dispatch",
        "\t",
        "\tenable_auto_inputs_fom(true);",
        "\tenable_non_manual_inputs_fom(true);",
        "\t//toggle_cycle_cost();",
        "\tenable('batt_cycle_cost', value('batt_cycle_cost_choice') == 1);",
        "",
        "\t// look ahead options",
        "\tlook_ahead = (mode == 0 || mode == 2);",
        "\tenable('batt_look_ahead_hours', look_ahead);",
        "\t\t",
        "\t",
        "}",
        "",
        "function enable_auto_inputs_fom(on_off)",
        "{",
        "\t//enable('batt_auto_gridcharge_max_daily', on_off);",
        "\tenable('batt_dispatch_update_frequency_hours', on_off);",
        "\tenable('batt_look_ahead_hours', on_off);",
        "\tenable('batt_cycle_cost_choice', on_off);",
        "\tenable('batt_cycle_cost', on_off);",
        "}",
        "",
        "function enable_non_manual_inputs_fom(on_off)",
        "{",
        "\tenable('batt_dispatch_auto_can_gridcharge', on_off);",
        "\tenable('batt_dispatch_auto_can_charge', on_off);",
        "\t//enable('batt_dispatch_auto_can_clipcharge', value('ui_batt_is_dc'));",
        "}",
        "",
        "function production_forecast_msg_auto()",
        "{",
        "\tmsg = '';",
        "\t",
        "\tif (technology() == 'Generic Battery' ) {",
        "\t\tmsg = 'For the Custom Forecast option, choose a generation profile at the simulation timestep to use for the dispatch forecast instead of the profile on the Power Plant page.';",
        "\t\tenable('batt_pv_ac_forecast_fom_auto',true);",
        "\t\tenable('batt_user_specified_weather_file',false);",
        "\t\tenable('batt_select_weather_file_forecast',false);",
        "\t}\r",
        "\telseif ( technology() == 'PVWatts Wind Battery Hybrid' \r",
        "\t      || technology() == 'PVWatts Wind FuelCell Battery Hybrid'\r",
        "\t      || technology() == 'Photovoltaic Wind Battery Hybrid'\r",
        "\t      || technology() == 'Generic PVWatts Wind FuelCell Battery Hybrid' ) {\r",
        "\t\tmsg = 'For the Custom Forecast option, choose a generation profile at the simulation timestep to use for the dispatch forecast instead of the combined generation of the wind and PV systems.';\r",
        "\t\tenable('batt_pv_ac_forecast_fom_auto',false);\r",
        "\t\tenable('batt_user_specified_weather_file',false);\r",
        "\t\tenable('batt_select_weather_file_forecast',false);\r",
        "\t}",
        "\telse {",
        "\t\tmsg = 'For the Custom Forecast option, choose a weather file in the SAM CSV format to use for the dispatch forecast instead of the weather file on the Location and Resource page.';\t",
        "\t\tenable('batt_pv_ac_forecast_fom_auto',false);",
        "\t\tenable('batt_user_specified_weather_file',true);",
        "\t\tenable('batt_select_weather_file_forecast',true);",
        "\t}",
        "\t",
        "\tproperty('batt_forecast_auto_description','Caption',msg);",
        "}",
        "",
        "function invalid_option_message()",
        "{",
        " \tif ((technology() == 'Standalone Battery' || technology() == \"MEwave Battery\") && value('batt_dispatch_wf_forecast_choice')==2) ",
        "\t{",
        "\t\tmsgbox('Invalid dispatch choice.\\nCustom forecast is not available for standalone or wave battery configurations.\\n\\nPlease choose a different automated dispatch option.');",
        "\t\tenable('batt_select_weather_file_forecast', false);",
        "\t\tenable('batt_user_specified_weather_file', false);",
        "\t\tenable('batt_pv_ac_forecast_fom_auto',false);",
        "\t}",
        "}"
    ]
