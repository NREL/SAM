        "on_load{'Marine Availability'} = define() {",
        "\tenable_avail();",
        "};",
        "",
        "function enable_avail()",
        "{",
        "\tif (financing() != \"Single Owner\") {",
        "\t\tcheck = value('wave_resource_model_choice');",
        "\t\tif (check == 1) {",
        "\t\t\tenable('me_adjust', true);",
        "\t\t\tproperty('me_avail_label', 'Caption', 'System availability losses reduce the system output to represent system outages or other events. Losses are applied in addition to the constant loss factors specified above');",
        "\t\t}",
        "\t\telse {",
        "\t\t\tenable('me_adjust', false);",
        "\t\t\tproperty('me_avail_label', 'Caption', 'System availability losses are only available for time series wave resource data.');",
        "\t\t\t",
        "\t\t}",
        "\t}",
        "\telse {",
        "\t\tenable('me_adjust', true);",
        "\t\tproperty('me_avail_label', 'Caption', 'System availability losses reduce the system output to represent system outages or other events. Losses are applied in addition to the constant loss factors specified above');",
