        "",
        "equations{ 'cost_dev_fee_value' } = define() {",
        "\treturn ${total_installed_cost} * ${cost_dev_fee_percent}/100.0; };",
        "",
        "equations{ 'cost_dev_fee_tax_liability' } = define () {",
