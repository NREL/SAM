    "Equations": [],
    "Callbacks": [
        "on_load{'Battery Dispatch Retail Rate'}=define()",
        "{\t",
        "\tvalue('batt_dispatch_choice', 4);",
        "\ttoggle_forecast_standalone_psd();",
        "\tpsd_forecast_choice();",
        "\tenable_auto_inputs_btm(1);",
        "\tproduction_forecast_msg();",
        "\trefresh();",
        "};",
        "",
        "on_change{'batt_dispatch_choice'} = define()",
        "{",
        "\ttoggle_dispatch();",
        "};",
        "",
        "on_change{'batt_cycle_cost_choice'} = define()",
        "{",
        "\tenable('batt_cycle_cost', ${batt_cycle_cost_choice} == 1);",
        "};",
        "",
        "on_change{'batt_select_weather_file_forecast_psd'}=define()",
        "{",
        "\tfile = choose_file( \"\", 'Choose a weather file', ",
        "\t\t\t'All Weather Files (*.tm2,*.tm3,*.epw,*.smw,*.csv)|*.tm2;*.tm3;*.epw;*.smw;*.csv');",
        "\t${batt_user_specified_weather_file_psd} = file;",
        "\trun_forecast(file);",
        "};",
        "",
        "function enable_auto_inputs_btm(on_off)",
        "{",
        "\tenable('batt_cycle_cost_choice', on_off);",
        "\tenable('batt_cycle_cost', ${batt_cycle_cost_choice} == 1);",
        "}",
        "",
        "function toggle_forecast_standalone_psd() ",
        "{",
        "\ttech = (technology() == 'Standalone Battery');",
        "\tenable('batt_dispatch_wf_forecast_choice_psd', tech == 0);",
        "}",
        "",
        "on_change{'batt_dispatch_wf_forecast_choice_psd'} = define() ",
        "{",
        "\tpsd_forecast_choice() ;",
        "};",
        "",
        "on_change{'batt_dispatch_load_forecast_choice_psd'} = define()",
        "{",
        "\tpsd_forecast_choice();",
        "};",
        "",
        "on_change{'batt_dispatch_load_escal_choice_psd'} = define()",
        "{",
        "\tpsd_forecast_choice();",
        "};",
        "",
        "on_change{'batt_pv_ac_forecast_psd'} = define()",
        "{",
        "\trun_generic_forecast(${batt_pv_ac_forecast_psd});",
        "};",
        "",
        "on_change{'load_user_forecast_data_psd'} = define()",
        "{",
        "\t${batt_load_ac_forecast} = ${load_user_forecast_data_psd};",
        "};",
        "",
        "function psd_forecast_choice() ",
        "{",
        "\tchoice = value('batt_dispatch_wf_forecast_choice_psd');",
        "\tenable('batt_select_weather_file_forecast_psd', choice == 2);",
        "\tenable('batt_user_specified_weather_file_psd', choice == 2);",
        "\tenable('batt_pv_ac_forecast_psd', choice == 2);",
        "",
        "\tvalue('batt_dispatch_wf_forecast_choice', choice);",
        "\tif (choice == 2)",
        "\t{",
        "\t\tac_forecast = value(\"batt_pv_ac_forecast\");",
        "\t\tclipping_forecast = value(\"batt_pv_clipping_forecast\");",
        "\t\tforecast_array = value(\"batt_pv_ac_forecast_psd\");",
        "\t\tif (#ac_forecast <= 1 || #clipping_forecast <= 1) { ",
        "\t\t\tif (technology() == 'PV Battery') {",
        "\t\t\t",
        "\t\t\t\tfile = ${batt_user_specified_weather_file_psd};",
        "\t\t\t\tif (file != \"\") {",
        "\t\t\t\t\trun_forecast(file);",
        "\t\t\t\t}",
        "\t\t\t}",
        "\t\t\telse {",
        "\t\t\t    // Generic system",
        "\t\t\t    if (#forecast_array > 1 && technology() == 'Custom Generation Battery') {\r",
        "\t\t\t\t\trun_generic_forecast(forecast_array);\r",
        "\t\t\t    }\r",
        "\t\t\t    // Don't do this for hybrids, we need a function that runs all the generators",
        "\t\t\t}",
        "\t\t}",
        "\t}",
        "\telse ",
        "\t{",
        "\t\t${batt_pv_ac_forecast} = [0];",
        "\t\t${batt_pv_clipping_forecast} = [0];",
        "\t}",
        "\t",
        "\tload_choice = value(\"batt_dispatch_load_forecast_choice_psd\");",
        "\tenable('load_user_forecast_data_psd', load_choice == 2);",
        "\tenable('batt_dispatch_load_escal_choice_psd', load_choice == 2);",
        "\tenable('batt_dispatch_load_forecast_escal_psd', load_choice == 2 && ${batt_dispatch_load_escal_choice_psd} == 1);",
        "",
        "\tvalue(\"batt_dispatch_load_forecast_choice\", load_choice);",
        "\t// Updates to load forecast vars happen in equations on Battery Dispatch Options BTM",
        "}",
        "",
        "function production_forecast_msg()",
        "{",
        "\tmsg = '';",
        "\t",
        "\tif (technology() == 'Custom Generation Battery' || is_hybrid()) {",
        "\t\tmsg = 'For the Custom Forecast option, choose a generation profile at the simulation timestep to use for the dispatch forecast instead of the profile on the Power Plant page.';",
        "\t\tshow('batt_pv_ac_forecast_psd',true);",
        "\t\tshow('batt_user_specified_weather_file_psd',false);",
        "\t\tshow('batt_select_weather_file_forecast_psd',false);",
        "",
        "\t}",
        "\telse {",
        "\t\tmsg = 'For the Custom Forecast option, choose a weather file in the SAM CSV format to use for the dispatch forecast instead of the weather file on the Location and Resource page.';\t",
        "\t\tshow('batt_pv_ac_forecast_psd',false);",
        "\t\tshow('batt_user_specified_weather_file_psd',true);",
        "\t\tshow('batt_select_weather_file_forecast_psd',true);",
        "\t}",
        "\t",
        "\tproperty('batt_price_signals_weather_file_desc','Caption',msg);",
        "}"
    ]
}