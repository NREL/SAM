    "Equations": [
        "equations{'batt_ui_inverter_eff'} = define() {",
        "\t// note PVWatts default inv_eff set to 96% in cmod_pvwattsv7.cpp var table",
        "\tif ( technology() != 'PV Battery' ) { return 96; }\t",
        "\tinv_model = ${inverter_model};",
        "\tif (inv_model == 0){ return ${inv_snl_eff_cec};\t\t}",
        "\telseif (inv_model == 1) { return ${inv_ds_eff}; }",
        "\telseif (inv_model == 2) { return ${inv_pd_eff}; }",
        "\telseif (inv_model == 3) { return ${inv_cec_cg_eff_cec}; }",
        "\telse { return 96; } ",
        "};"
    ],
    "Callbacks": [
        "on_load{'Battery Configuration'} = define()",
        "{",
        "\tmsg = '';",
        "\t// for ac-only configs, set to AC-connected",
        "\tif ( !is_dc_tech() ) // AC-connected only",
        "\t{",
        "\t\tvalue('batt_ac_or_dc',1);",
        "\t\tmsg = 'For the Generic Battery, Standalone Battery, MEwave Battery, and Fuel Cell configurations, the battery can only be AC-connected.';",
        "\t}",
        "\telse // PV Battery can be DC- or AC-connected",
        "\t{",
        "\t\tmsg = 'For the PV Battery configuration, the battery can be connected either to the DC or AC side of the PV inverter.';",
        "\t}",
        "\tproperty('lbl_dc_ac','Caption',msg);\t",
        "\tenable_inputs();",
        "};",
        "",
        "on_change{'batt_ac_or_dc'} = define()",
        "{",
        "\tenable_inputs();",
        "\tif (is_dc_tech() && behind_the_meter()) {",
        "\t\tenable_disable_powerflow_options();",
        "\t}",
        "};",
        "",
        "function is_dc_tech()",
        "{",
        "\ttech = technology();",
        "\treturn !(tech == 'Generic Battery' || tech == 'Fuel Cell' || tech == 'Standalone Battery' || tech == 'MEwave Battery'); // AC-connected only",
        "}",
        "",
        "function enable_inputs()",
        "{",
        "\tif ( is_dc_tech() ) ",
        "\t{",
        "\t\tenable('batt_ac_or_dc',true);",
        "\t\tisdc = ( value('batt_ac_or_dc') == 0 );",
        "\t\tenable('batt_dc_dc_efficiency', isdc);",
        "\t\tenable('batt_inverter_efficiency_cutoff',isdc);",
        "\t\tenable('batt_ui_inverter_eff',isdc);",
        "\t\tenable('batt_ac_dc_efficiency', !isdc);",
        "\t\tenable('batt_dc_ac_efficiency', !isdc);",
        "\t\t",
        "\t\tif (isdc) { msg = 'The DC-connected battery battery stops charging or discharging when the PV inverter operating efficiency is less than the inverter efficiency cutoff.'; }",
        "\t\telse { msg = ''; }",
        "\t\tproperty('lbl_dc_connected','Caption',msg);",
        "\t}",
        "\telse",
        "\t{",
        "\t\tenable('batt_ac_or_dc',false);",
        "\t\tenable('batt_dc_dc_efficiency',false);",
        "\t\tenable('batt_inverter_efficiency_cutoff',false);",
        "\t\tenable('batt_ui_inverter_eff',false);",
        "\t}",
        "}",
        ""
    ]
}