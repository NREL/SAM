    "Equations": [
        "equations{ 'ui_battery_cost' } = define() {\r",
        "\treturn ${battery_total_installed_cost};\r",
        "};\r",
        "\r",
        "equations{ 'ui_wind_cost' } = define() {\r",
        "\treturn ${wind_total_installed_cost};\r",
        "};\r",
        "\r",
        "equations{ 'ui_pv_cost' } = define() {\r",
        "\treturn ${pv_total_installed_cost};\r",
        "};\r",
        "\r",
        "equations{ 'ui_fuel_cell_cost' } = define() {\r",
        "\tif ( technology() == 'PVWatts Wind FuelCell Battery Hybrid' \r",
        "\t  || technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' ) {\r",
        "\t\treturn ${fuelcell_total_installed_cost};\r",
        "\t}\r",
        "\telse {\r",
        "\t\treturn 0;\r",
        "\t}\r",
        "};\r",
        "\r",
        "equations{ 'ui_customgeneration_cost' } = define() {\r",
        "\tif ( technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' ) {\r",
        "\t\treturn ${customgeneration_total_installed_cost};\r",
        "\t}\r",
        "\telse {\r",
        "\t\treturn 0;\r",
        "\t}\t\r",
        "};\r",
        "\r",
        "equations{ 'total_installed_cost' } = define() {\r",
        "\tif ( technology() == 'PVWatts Wind Battery Hybrid' ) {\r",
        "\t\treturn ${hybrid_capital_cost}\r",
        "\t\t     + ${pv_total_installed_cost} \r",
        "             + ${wind_total_installed_cost} \r",
        "             + ${battery_total_installed_cost};\r",
        "\t}\r",
        "\telseif ( technology() == 'PVWatts Wind FuelCell Battery Hybrid' ) {\r",
        "\t\treturn  ${hybrid_capital_cost}\r",
        "\t\t     + ${pv_total_installed_cost} \r",
        "             + ${wind_total_installed_cost} \r",
        "             + ${battery_total_installed_cost} \r",
        "             + ${fuelcell_total_installed_cost} \r",
        "             + ${battery_total_installed_cost};\r",
        "\t}\r",
        "\telseif ( technology() == 'Photovoltaic Wind Battery Hybrid' ) {\r",
        "\t\treturn ${hybrid_capital_cost}\r",
        "\t\t     + ${pv_total_installed_cost} \r",
        "             + ${wind_total_installed_cost} \r",
        "             + ${battery_total_installed_cost};\r",
        "\t}\r",
        "\telseif ( technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' ) {\r",
        "\t\treturn ${hybrid_capital_cost}\r",
        "\t\t     + ${customgeneration_total_installed_cost} \r",
        "             + ${pv_total_installed_cost} \r",
        "             + ${wind_total_installed_cost}  \r",
        "             + ${fuelcell_total_installed_cost} \r",
        "             + ${battery_total_installed_cost};\r",
        "\t}\r",
        "\telse {\r",
        "\t\treturn null;\r",
        "\t}\r",
        "};\r",
        ""
    ],
    "Callbacks": [
        "on_load{'Hybrid Costs'} = define() {\r",
        "\tis_customgeneration_tech = technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid';\r",
        "\tis_fuel_cell_tech = ( technology() == 'PVWatts Wind FuelCell Battery Hybrid' || technology() == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' ) ;\r",
        "\tshow( 'ui_customgeneration_cost',  is_customgeneration_tech );\r",
        "\tshow( 'ui_fuel_cell_cost', is_fuel_cell_tech );\r",
        "\tvalue('total_installed_cost', value('total_installed_cost'));\r",
        "\trefresh('total_installed_cost');\r",
        "\trefresh();\r",
        "};"
    ]
