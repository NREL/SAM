    "Equations": [
        "equations{ '$MIMO$ thermalrate' } = define() {",
        "",
        "\tfin = financing();",
        "\ttech = technology();",
        "\tif(fin == \"Commercial\")",
        "\t{",
        "\t\tif (${thermal_buy_rate_option} == 0)",
        "\t\t{",
        "\t\t\tbuy_rate = alloc(8760);",
        "\t\t\tfor (i=0;i<#buy_rate; i++)",
        "\t\t\t\tbuy_rate[i] = ${thermal_buy_rate_flat};",
        "\t\t}",
        "\t\telse if (${thermal_buy_rate_option} == 1)",
        "\t\t{",
        "\t\t\tbuy_rate = ${thermal_timestep_buy_rate};",
        "\t\t}",
        "\t\telse ",
        "\t\t{",
        "\t\t\tbuy_rate = alloc(8760);",
        "\t\t\tfor (i=0;i<#buy_rate; i++)",
