    "Equations": [
        "equations{ 'wind_resource.requested_ht' } = define() {",
        "\treturn ${wind_turbine_hub_ht}; };",
        "",
        "equations{'wind_resource_filename'} = define() {",
        "\tif ( ${use_specific_wf_wind} ) return ${user_specified_wf_wind};",
        "\telse return ${wind_resource.file};",
        "};"
    ],
    "Callbacks": [
        "on_change{'btnScanLibrary'} = define() {",
        "\trescanlibrary('wind');",
        "};",
        "",
        "on_load{'Wind Resource File'} = define() {",
        "\ton_change{'use_specific_wf_wind'}();\r",
        "\ton_change{'wind_resource.location'}();",
        "};",
        "",
        "on_change{'wind_resource.location'} = define() {",
        "\tfile = ${wind_resource.file};",
        "\t",
        "\tif (!file_exists(file))",
        "\t{",
        "\t\trescanlibrary( 'wind' );",
        "\t\trefresh('wind_resource.location');",
        "\t\treturn;",
        "\t}",
        "",
        "\tfile = replace(file, '\\\\', '/');",
        "\tfn = open(file,'r');",
        "",
        "\tclose(fn);",
        "};",
        "",
        "on_change{'btnWindLocationLookup'} = define() {",
        "\tfile = windtoolkit();",
        "\t//do nothing if they cancel the window",
        "\tif ( file == null || file == \"\" ) ",
        "\t\treturn;",
        "\t\t",
        "\tobj = ssc_create();",
        "\t//check that the file downloaded in a valid format",
        "\tssc_var( obj, 'file_name', file);",
        "\tssc_var(obj, 'scan_header_only', 1);",
        "\t//hard code in a hub height of 80 just for checking the file",
        "\tssc_var(obj, 'requested_ht', 80);",
        "\terr = ssc_exec(obj, 'wind_file_reader');",
        "\tif (err != 0) //file didn't run successfully",
        "\t{\r",
        "\t\tmsgbox('Wind resource file check failed!\\n'\r",
        "\t\t       + 'First 200 characters of file contents:\\n ' \r",
        "\t\t       +  mid(read_text_file(file),0,200)\r",
        "\t\t \t   + '\\n\\n' + file);",
        "\t\treturn;",
        "\t}",
        "\tssc_free(obj);",
        "\t",
        "\t//otherwise, everything went fine, assign the file",
        "\tvalue( 'user_specified_wf_wind', file );",
        "\tvalue( 'use_specific_wf_wind', 1 );",
        "\tmsgbox(\"Success!\\nFile downloaded from NREL WIND Toolkit:\\n\\n\" + file);",
        "\tenable( 'user_specified_wf_wind', 1);",
        "\tenable( 'select_wf_wind', 1);",
        "};",
        "",
        "on_change{'use_specific_wf_wind'} = define() {",
        "\tenable( 'user_specified_wf_wind', ${use_specific_wf_wind} );",
        "\tenable( 'select_wf_wind', ${use_specific_wf_wind} );",
        "};",
        "",
        "on_change{'select_wf_wind'} = define() {",
        "\tfile = choose_file( path_only( value('user_specified_wf_wind') ), 'Choose a wind resource file (srw or csv)', ",
        "\t\t'Wind Resource Files (*.*)|*.*');",
        "\t\t",
        "\tif ( file != '' ) {",
        "\t\tfile = replace(file, '\\\\', '/');",
        "\t\tvalue( 'user_specified_wf_wind', file );",
        "\t}",
        "};"
    ]
}