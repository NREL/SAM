	
	
on_load{"PVWatts"}=define(){
	on_change{"pvwatts.tilt_eq_lat"}();
	on_change{"pvwatts.use_poa"}();
};	

on_change{"pvwatts.tilt_eq_lat"} = define() {
	val = value("pvwatts.tilt_eq_lat");
	enable( "pvwatts.tilt", val == 0 );
};

on_change{"pvwatts.use_poa"} = define() {
	val = value("pvwatts.use_poa");
	enable( "pvwatts.poa_input", val != 0 );
};