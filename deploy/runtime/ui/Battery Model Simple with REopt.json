    "Callbacks": [
        "on_load{'Battery Model Simple with REopt'} = define(){",
        "\tenable(\"batt_custom_dispatch\", (value(\"batt_simple_dispatch\") == 2));",
        "\tbatt_loss_adjustment_set();\r",
        "};",
        "",
        "on_change{\"batt_simple_dispatch\"} = define(){",
        "\tenable(\"batt_custom_dispatch\", (value(\"batt_simple_dispatch\") == 2));",
        "};",
        "",
        "on_change{'call_reopt'} = define(){",
        "\tdispatch_original_mode = value(\"batt_simple_dispatch\"); //save original dispatch mode",
        "\tbatt_kw_original = value(\"batt_simple_kw\");",
        "\tbatt_kwh_original = value(\"batt_simple_kwh\");",
        "\tur_dc_tou_mat_original = value('ur_dc_tou_mat');\r",
        "\tur_dc_flat_mat_original = value('ur_dc_flat_mat');\r",
        "\tur_ec_tou_mat_original = value('ur_ec_tou_mat');\r",
        "\t",
        "\twf_len = 60 / ${ui_step_minutes} * 8760;",
        "\tload_len = #(${load});",
        "\tif (wf_len != load_len) {",
        "\t\tmsgbox('Time step issue.\\nWeather and load data must have the same time step.');",
        "\t\texit;",
        "\t}",
        "\tif (load_len != 8760 * 4 && load_len != 8760 * 2 && load_len != 8760) \r",
        "\t{\r",
        "\t\tmsgbox('Unsupported simulation time step.\\nWeather file time step is ' + to_string(${ui_step_minutes}) + ' minutes. REopt requires 15-, 30-, or 60-minute time steps.');\r",
        "\t\texit;\r",
        "\t}",
        "\t// reset size to 0 to remove battery costs from total_installed_cost",
        "\tvalue(\"batt_simple_kw\", 0);",
        "\tvalue(\"batt_simple_kwh\", 0); ",
        "\tvalue(\"batt_simple_dispatch\",0); //set dispatch mode to 0 to run ReOPT",
        "\t\r",
        "\t// set energy and demand tier max value to 1e14 to avoid issue with HiGHS optimization\r",
        "\ttier_max = 1e14;\r",
        "\tvalue('ur_dc_tou_mat', set_max_value('ur_dc_tou_mat', tier_max));\r",
        "\tvalue('ur_dc_flat_mat', set_max_value('ur_dc_flat_mat', tier_max));\r",
        "\tvalue('ur_ec_tou_mat', set_max_value('ur_ec_tou_mat', tier_max));\r",
        "\t",
        "\treopt = reopt_size_battery();",
        "\ty = reopt.response;",
        "\tif (reopt.error){",
        "\t\tif (strlen(reopt.error) > 0){",
        "\t\t\tmsgbox(reopt.error);",
        "\t\t}",
        "\t\telse{",
        "\t\t\tmsgbox(\"REopt API timed out. Please try again later.\");",
        "\t\t}",
        "\t\tvalue(\"batt_simple_dispatch\",dispatch_original_mode); //restore original dispatch mode for early exit from ReOPT",
        "\t\tvalue(\"batt_simple_kw\", batt_kw_original);",
        "\t\tvalue(\"batt_simple_kwh\", batt_kwh_original);\r",
        "\t\tvalue('ur_dc_tou_mat', ur_dc_tou_mat_original);\r",
        "\t\tvalue('ur_dc_flat_mat', ur_dc_flat_mat_original);\r",
        "\t\tvalue('ur_ec_tou_mat', ur_ec_tou_mat_original); ",
        "\t\texit;",
        "\t}",
        "",
        "\tif (y.outputs == null){",
        "\t\tmsgbox(\"Error in REopt response. Try again later or try another system design.\");",
        "\t\tvalue(\"batt_simple_dispatch\",dispatch_original_mode); //restore original dispatch mode for early exit from ReOPT",
        "\t\tvalue(\"batt_simple_kw\", batt_kw_original);",
        "\t\tvalue(\"batt_simple_kwh\", batt_kwh_original);\r",
        "\t\tvalue('ur_dc_tou_mat', ur_dc_tou_mat_original);\r",
        "\t\tvalue('ur_dc_flat_mat', ur_dc_flat_mat_original);\r",
        "\t\tvalue('ur_ec_tou_mat', ur_ec_tou_mat_original);  ",
        "\t\texit;",
        "\t}",
        "\t",
        "\tpv = y.outputs.PV;",
        "\tbatt = y.outputs.ElectricStorage;",
        "\telec = y.outputs.ElectricUtility;",
        "",
        "\tif (batt == null || (batt.size_kw == null && batt.size_kwh == null)){",
        "\t\tapply_str = \"Apply results?\\nOptimal capacity (kWh): \" + to_string(0) + ",
        "\t\t\t\"\\nOptimal power (kW): \" + to_string(0) + \"\\n\\nReplace \" + ",
        "\t\t\t\"'Battery capacity' and 'Battery power' with results from REopt?\";",
        "\t\tapply = yesno(apply_str);",
        "\t\tif (apply) {",
        "\t\t\tvalue('batt_simple_kw', 0.0);",
        "\t\t\tvalue('batt_simple_kwh', 0.0);\r",
        "\t\t\tvalue('ur_dc_tou_mat', ur_dc_tou_mat_original);\r",
        "\t\t\tvalue('ur_dc_flat_mat', ur_dc_flat_mat_original);\r",
        "\t\t\tvalue('ur_ec_tou_mat', ur_ec_tou_mat_original); ",
        "\t\t}\r",
        "\t\telse {",
        "\t\t\tvalue(\"batt_simple_dispatch\",dispatch_original_mode); //restore original dispatch mode for early exit from ReOPT",
        "\t\t\tvalue(\"batt_simple_kw\", batt_kw_original);",
        "\t\t\tvalue(\"batt_simple_kwh\", batt_kwh_original); \r",
        "\t\t\tvalue('ur_dc_tou_mat', ur_dc_tou_mat_original);\r",
        "\t\t\tvalue('ur_dc_flat_mat', ur_dc_flat_mat_original);\r",
        "\t\t\tvalue('ur_ec_tou_mat', ur_ec_tou_mat_original); \r",
        "\t\t}",
        "\t\texit;",
        "\t}",
        "",
        "\tapply_str = \"Apply results?\\nOptimal capacity (kWh): \" + to_string(batt.size_kwh) + ",
        "\t\t\"\\nOptimal power (kW): \" + to_string(batt.size_kw) + \"\\n\\nReplace \" +",
        "\t\t\"'Battery capacity', 'Battery power', and 'Custom dispatch' with results from REopt?\";",
        "\tapply = yesno(apply_str);",
        "\tif (!apply) {",
        "\t\tvalue(\"batt_simple_dispatch\",dispatch_original_mode); //restore original dispatch mode for early exit from ReOPT",
        "\t\tvalue(\"batt_simple_kw\", batt_kw_original);",
        "\t\tvalue(\"batt_simple_kwh\", batt_kwh_original);\r",
        "\t\tvalue('ur_dc_tou_mat', ur_dc_tou_mat_original);\r",
        "\t\tvalue('ur_dc_flat_mat', ur_dc_flat_mat_original);\r",
        "\t\tvalue('ur_ec_tou_mat', ur_ec_tou_mat_original);  ",
        "\t\texit;",
        "\t}",
        "\t",
        "\tvalue('batt_simple_kw', batt.size_kw);",
        "\tvalue('batt_simple_kwh', batt.size_kwh);\r",
        "\tvalue('ur_dc_tou_mat', ur_dc_tou_mat_original);\r",
        "\tvalue('ur_dc_flat_mat', ur_dc_flat_mat_original);\r",
        "\tvalue('ur_ec_tou_mat', ur_ec_tou_mat_original); ",
        "\t",
        "\tif ((batt.size_kw == 0 && batt.size_kwh == 0) || !apply) {",
        "\t\tvalue(\"batt_simple_dispatch\",dispatch_original_mode); //restore original dispatch mode for early exit from ReOPT",
        "\t\texit;",
        "\t}",
        "\t",
        "\tbatt_to_grid = null;",
        "\tif (batt.year_one_to_grid_series_kw) {",
        "\t\tbatt_to_grid = batt.year_one_to_grid_series_kw;\r",
        "\t}\t",
        "\tbatt_to_load = batt.storage_to_load_series_kw;",
        "\tpv_charge = pv.electric_to_storage_series_kw;",
        "\tgrid_charge = elec.electric_to_storage_series_kw;",
        "",
        "",
        "\tvalue('batt_simple_dispatch', 2);",
        "",
        "\tload = ${load};",
        "\t",
        "\tdispatch = alloc(#load);",
        "\tfor (i=0; i<#grid_charge; i++){",
        "\t\tif (batt_to_grid){",
        "\t\t\tdischarge = batt_to_grid[i] + batt_to_load[i];",
        "\t\t} else{",
        "\t\t\tdischarge = batt_to_load[i];",
        "\t\t}",
        "\t\tcharge = -pv_charge[i] - grid_charge[i];",
        "\t\t",
        "\t\tdispatch[i] = discharge + charge;",
        "\t}",
        "",
        "\tvalue('batt_custom_dispatch', dispatch);",
        "};\r",
        "\r",
        "// set maximum value of all numbers in energy and demand rate table\r",
        "function set_max_value(var_name,max_value) {\r",
        "\tx = value(var_name);\r",
        "\tresult = x;\r",
        "\tfor (i=0; i<#x; i++) {\r",
        "\t\ty = x[i];\r",
        "\t\tfor (j=0; j<#y; j++ ) {\r",
        "\t\t\tif ( y[j] > max_value ) {\r",
        "\t\t\t\tresult[i][j] = max_value;\r",
        "\t\t\t}\r",
        "\t\t}\r",
        "\t}\r",
        "\treturn result;\r",
        "}\r",
        "\r",
        "function batt_loss_adjustment_set() \r",
        "{\r",
        "\tif (financing() != \"LCOE Calculator\" && financing() != \"None\") {\r",
        "\t\tif (value('system_use_lifetime_output') == 1) {\r",
        "\t\t\tproperty('batt_adjust', 'AnalysisPeriod', value('analysis_period'));\r",
        "\t\t}\r",
        "\t\telse {\r",
        "\t\t\tproperty('batt_adjust', 'AnalysisPeriod', 1);\r",
        "\t\t}\r",
        "\t}\r",
        "\telse {\r",
        "\t\tproperty('batt_adjust', 'AnalysisPeriod', 1);\r",
        "\t}\r",
        "}"
    ]
}