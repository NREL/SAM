    "Equations": [],
    "Callbacks": [
        "on_load{'Battery Dispatch Grid Power Targets'}=define()",
        "{\t",
        "\tvalue('batt_dispatch_choice', 1);",
        "\ttoggle_dispatch_target();\t",
        "};",
        "",
        "on_change{'batt_target_choice'} = define()",
        "{",
        "\ttoggle_dispatch_target();",
        "};",
        "",
        "function toggle_dispatch_target()",
        "{",
        "\tmode = ${batt_dispatch_choice};",
        "\tif (mode != 1)",
        "\t\tenable_target_inputs(false);",
        "\telse",
        "\t\tenable_target_inputs(true);",
        "}",
        "",
        "function enable_target_inputs(on_off)",
        "{",
        "\tif (!on_off)",
        "\t{",
        "\t\tenable('batt_target_choice',on_off);",
        "\t\tenable('batt_target_power',on_off);",
        "\t\tenable('batt_target_power_monthly',on_off);",
        "\t}",
        "\telse ",
        "\t{",
        "\t\tenable('batt_target_choice',true);",
        "\t\tmode = ${batt_target_choice};",
        "\t\tif (mode == 0)",
        "\t\t{",
        "\t\t\tenable('batt_target_power',false);",
        "\t\t\tenable('batt_target_power_monthly',true);",
        "\t\t}",
        "\t\telse",
        "\t\t{",
        "\t\t\tenable('batt_target_power',true);",
        "\t\t\tenable('batt_target_power_monthly',false);",
        "\t\t}",
        "\t}",
        "}",
        ""
    ]
