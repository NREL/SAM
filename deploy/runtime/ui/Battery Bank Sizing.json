    "Equations": [
        "equations{'batt_bank_power_dc_ac'} = define() {",
        "\treturn ${batt_bank_size_dc_ac};",
        "};",
        "",
        "equations{'batt_bank_size'} = define() {",
        "\tif (${batt_duration_choice}==0) {",
        "\t\treturn ${batt_bank_size_ui};",
        "\t}",
        "\telse",
        "\t\treturn ${batt_bank_power} * ${batt_bank_duration};",
        "};"
    ],
    "Callbacks": [
        "on_load{'Battery Bank Sizing'} = define()",
        "{",
        "\tset_chemistry();",
        "\tset_bank_sizing();",
        "\t//set_duration_choice();",
        "\tis_cap = (value('batt_duration_choice') == 0);",
        "\t",
        "\tshow('batt_bank_duration',!is_cap);",
        "\tshow('batt_bank_size_ui',is_cap);",
        "\trefresh();",
        "\t",
        "\ton_change{'batt_size_choice'};",
        "\ton_change{'batt_bank_size'};",
        "\ton_change{'batt_bank_power'};",
        "\ton_change{'bank_bank_size_specify'};",
        "\ton_change{'batt_duration_choice'};",
        "\ton_change{'batt_bank_duration'};",
        "};",
        "on_change{'batt_size_choice'} = define()",
        "{",
        "\tset_bank_sizing();",
        "\tbattsize_warning_check();",
        "};",
        "on_change{'batt_bank_size'} = define(){",
        "\tbattsize_warning_check();",
        "};",
        "on_change{'batt_bank_power'} = define(){",
        "\tbattsize_warning_check();",
        "};",
        "on_change{'batt_bank_size_specify'} = define(){battsize_warning_check();};",
        "on_change{'batt_duration_choice'} = define()",
        "{",
        "\tset_duration_choice();",
        "\tbattsize_warning_check();",
        "};",
        "on_change{'batt_bank_duration'} = define()",
        "{",
        "\t",
        "\tis_cap = (value('batt_duration_choice') == 0);",
        "\tif (!is_cap) {",
        "\t\tbank_power = value('batt_bank_power');",
        "\t\tbank_duration = value('batt_bank_duration');",
        "\t\tvalue('batt_bank_size_ui',bank_power * bank_duration);",
        "\t}",
        "\tbattsize_warning_check();",
        "};",
        "function set_bank_sizing()",
        "{",
        "\tisauto = (value('batt_size_choice') == 0);",
        "\tisflow = is_flow();",
        "",
        "\t// auto-sizing",
        "\tenable('batt_bank_size', isauto);",
        "\tenable('batt_bank_size_ui', isauto);",
        "\tenable('batt_bank_power', isauto);",
        "\tenable('batt_bank_voltage', isauto);",
        "\tenable('batt_bank_size_dc_ac', isauto);",
        "\tenable('batt_duration_choice', isauto);",
        "\tenable('batt_bank_duration', isauto);",
        "\t// manual specify",
        "\tshow('batt_bank_size_specify', isflow);",
        "\tshow('batt_C_rate_max_charge_input', !isflow);",
        "\tshow('batt_C_rate_max_discharge_input', !isflow);",
        "",
        "\tenable('batt_bank_ncells_serial', !isauto);",
        "\tenable('batt_bank_nstrings', !isauto);",
        "\tenable('batt_bank_nseries_stacks', !isauto);",
        "\t",
        "",
        "\tif (isflow)",
        "\t\tenable('batt_bank_size_specify', !isauto);",
        "\telse",
        "\t{",
        "\t\tenable('batt_C_rate_max_charge_input', !isauto);",
        "\t\tenable('batt_C_rate_max_discharge_input', !isauto);",
        "\t}",
        "}",
        "",
        "function set_duration_choice()",
        "{",
        "\tis_cap = (value('batt_duration_choice') == 0);",
        "\t",
        "\tshow('batt_bank_duration',!is_cap);",
        "\tshow('batt_bank_size_ui',is_cap);",
        "\tbank_power = value('batt_bank_power');",
        "\tbank_cap = value('batt_bank_size_ui');",
        "\tif (!is_cap)",
        "\t{",
        "\t\tduration = bank_cap / bank_power;",
        "\t\tvalue('batt_bank_duration', duration);",
        "\t}",
        "\telse",
        "\t{",
        "\t\tduration = value('batt_bank_duration');",
        "\t\tbank_cap_calc = duration * bank_power;",
        "\t\tvalue('batt_bank_size_ui', bank_cap_calc);",
        "\t}",
        "\t",
        "\trefresh();",
        "}",
        "",
        "function calc_batt_size() {",
        "\tis_cap = (value('batt_duration_choice') == 0);",
        "\tif (is_cap) {",
        "\t\tbank_cap = value('batt_bank_size');",
        "\t\tbank_power = value('batt_bank_power');",
        "\t\tbank_duration = bank_cap / bank_power;",
        "\t\tvalue('batt_bank_duration',batt_duration);",
        "\t}",
        "\telse {",
        "\t\tbank_duration = value('batt_bank_duration');",
        "\t\tbank_power = value('batt_bank_power');",
        "\t\tbank_cap = bank_power * bank_duration;",
        "\t\tvalue('batt_bank_size', bank_cap);",
        "\t}",
        "}",
        "\t"
    ]
}