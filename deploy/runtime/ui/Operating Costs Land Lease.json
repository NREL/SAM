    "Equations": [
        "// om_land_lease in $/acre",
        "equations{ 'om_land_lease'} = define(){",
        "\tif ( ${ui_land_area_units} == 1 )",
        "\t{",
        "\t\tfor ( i=0; i<#${ui_land_lease}; i++)",
        "\t\t{",
        "\t\t\tx[i] = ${ui_land_lease}[i] / 2.47105;",
        "\t\t}",
        "\t}",
        "\telse",
        "\t{",
        "\t\tx = ${ui_land_lease};",
        "\t}",
        "\treturn x;",
        "};",
        "",
        "// total_land_area must be defined on System Design form",
        "// or equivalent for each technology",
        "equations{ 'land_area' } = define() {",
        "\ttech = technology();",
        "\tif ( tech == 'Empirical Trough' ) { \r",
        "\t\tland_area = ${ui_total_land_area}; }",
        "\telseif ( tech == 'MSPT') {\r",
        "\t\tland_area = ${csp.pt.sf.total_land_area}; }",
        "\telseif ( tech == 'Generic CSP System') {\r",
        "\t\tland_area = ${csp.gss.solf.total_land_area}; }",
        "\telseif ( tech == 'MSLF' || tech == 'MSLF IPH') {\r",
        "\t\tland_area = ${SF_CALC_total_land_area}; }",
        "\telseif ( tech == 'DSLF') {\r",
        "\t\tland_area = ${csp.lf.cost.total_land_area}; }\t",
        "\telseif ( tech == 'High-X Concentrating PV') {\r",
        "\t\tland_area = ${hcpv.array.total_land_area}; }",
        "\telseif ( tech == 'Flat Plate PV' \r",
        "\t      || tech == 'PVWatts' \r",
        "\t      || tech == 'Physical Trough' \r",
        "\t      || tech == 'PV Battery' \r",
        "\t      || tech == 'Fuel Cell' \r",
        "\t      || tech == 'PVWatts Wind Battery Hybrid' \r",
        "\t      || tech == 'PVWatts Wind FuelCell Battery Hybrid' \r",
        "\t      || tech == 'Photovoltaic Wind Battery Hybrid' \r",
        "\t      || tech == 'CustomGeneration PVWatts Wind FuelCell Battery Hybrid' ) {\r",
        "\t\tland_area = ${total_land_area}; }",
        "\telse {\r",
        "\t\tland_area = 0; }",
        "\treturn land_area;",
        "};",
        "",
        "equations{ 'ui_land_area_ha' } = define()",
        "{",
        "\treturn ${land_area} / 2.47105;",
        "};"
    ],
    "Callbacks": [
        "on_load{'Operating Costs Land Lease'} = define() {",
        "\tenable_lease_escalation();",
        "};",
        "",
        "on_change{'ui_land_lease'} = define() {",
        "\tenable_lease_escalation();",
        "};",
        "",
        "on_change{'ui_land_area_units'} = define() {",
        "\tenable_lease_escalation();",
        "};",
        "",
        "function enable_lease_escalation() ",
        "{",
        "\tenable( 'om_land_lease_escal',#${ui_land_lease}==1);",
        "\tunits=['$/acre','$/ha'];",
        "\tproperty('ui_land_lease','Label',units[${ui_land_area_units}]);",
        "\tproperty( 'ui_land_lease','FixedLength',${analysis_period});",
        "}",
        ""
