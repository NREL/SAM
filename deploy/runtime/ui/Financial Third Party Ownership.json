    "Equations": [
        "equations{ 'nominal_discount_rate' } = define() {",
        "\treturn ( (1.0 + ${real_discount_rate}/100.0) * (1.0 + ${inflation_rate}/100.0) - 1.0 ) * 100.0; ",
        "};",
        "",
        ""
    ],
    "Callbacks": [
        "function lease_or_ppa()",
        "{",
        "\tlease = (value('lease_or_ppa') == 0); //0=lease, 1=ppa",
        "\tenable('lease_price', lease);",
        "\tenable('lease_escalation', lease);",
        "\tenable('ppa_price', !lease);",
        "\tenable('ppa_escalation', !lease);\t",
        "}",
        "",
        "on_load{'Financial Third Party Ownership'} = define() {",
        "\tlease_or_ppa();",
        "};",
        "",
        "on_change{'lease_or_ppa'} = define() {",
        "\tlease_or_ppa();",
        "};",
