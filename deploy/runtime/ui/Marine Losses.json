    "Callbacks": [
        "function check_mhk_losses(widget_name)",
        "{",
        "\ttotal_loss = to_real(value(\"loss_additional\"))",
        "\t+to_real(value(\"loss_downtime\"))",
        "\t+to_real(value(\"loss_transmission\"))",
        "\t+to_real(value(\"loss_resource_overprediction\"))",
        "\t+to_real(value(\"loss_array_spacing\"));",
        "\tx = to_real(value(widget_name));",
        "\tif (x < 0.0)",
        "\t{ ",
        "\t\tx=0.0;",
        "\t\tmsgbox(\"Loss must be greater to or equal to zero.\");",
        "\t}",
        "\telse if (total_loss > 100.0)",
        "\t{",
        "\t\tx = 100.0*x/total_loss;",
        "\t\tif (widget_name != \"loss_additional\")",
        "\t\t{",
        "\t\t\ty= to_real(value(\"loss_additional\"));",
        "\t\t\ty=100.0*y/total_loss;",
        "\t\t\tvalue(\"loss_additional\",y, false);",
        "\t\t\trefresh(\"loss_additional\");",
        "\t\t}",
        "\t\tif (widget_name != \"loss_downtime\")",
        "\t\t{",
        "\t\t\ty= to_real(value(\"loss_downtime\"));",
        "\t\t\ty=100.0*y/total_loss;",
        "\t\t\tvalue(\"loss_downtime\",y, false);",
        "\t\t\trefresh(\"loss_downtime\");",
        "\t\t}",
        "\t\tif (widget_name != \"loss_transmission\")",
        "\t\t{",
        "\t\t\ty= to_real(value(\"loss_transmission\"));",
        "\t\t\ty=100.0*y/total_loss;",
        "\t\t\tvalue(\"loss_transmission\",y, false);",
        "\t\t\trefresh(\"loss_transmission\");",
        "\t\t}",
        "\t\tif (widget_name != \"loss_resource_overprediction\")",
        "\t\t{",
        "\t\t\ty= to_real(value(\"loss_resource_overprediction\"));",
        "\t\t\ty=100.0*y/total_loss;",
        "\t\t\tvalue(\"loss_resource_overprediction\",y, false);",
        "\t\t\trefresh(\"loss_resource_overprediction\");",
        "\t\t}",
        "\t\tif (widget_name != \"loss_array_spacing\")",
        "\t\t{",
        "\t\t\ty= to_real(value(\"loss_array_spacing\"));",
        "\t\t\ty=100.0*y/total_loss;",
        "\t\t\tvalue(\"loss_array_spacing\",y, false);",
        "\t\t\trefresh(\"loss_array_spacing\");",
        "\t\t}",
        "\t\tmsgbox(\"Total loss > 100%, so losses scaled so that total is 100%.\");",
        "\t}",
        "\tvalue(widget_name, x, false);",
        "\trefresh(widget_name);",
        "\ttotal_loss = to_real(value(\"loss_additional\"))",
        "\t+to_real(value(\"loss_downtime\"))",
        "\t+to_real(value(\"loss_transmission\"))",
        "\t+to_real(value(\"loss_resource_overprediction\"))",
        "\t+to_real(value(\"loss_array_spacing\"));",
        "\tvalue(\"loss_total\", total_loss,false);",
        "\trefresh(\"loss_total\");",
        "}",
        "",
        "on_change{'loss_additional'}=define()",
        "{",
        "\tcheck_mhk_losses(\"loss_additional\");",
        "};",
        "",
        "",
        "on_change{'loss_downtime'}=define()",
        "{",
        "\tcheck_mhk_losses('loss_downtime');",
        "};",
        "",
        "on_change{'loss_transmission'}=define()",
        "{",
        "\tcheck_mhk_losses('loss_transmission');",
        "};",
        "",
        "on_change{'loss_resource_overprediction'}=define()",
        "{",
        "\tcheck_mhk_losses('loss_resource_overprediction');",
        "};",
        "",
        "on_change{'loss_array_spacing'}=define()",
        "{",
        "\tcheck_mhk_losses('loss_array_spacing');",
        "};",
        "",
        ""
    ]
