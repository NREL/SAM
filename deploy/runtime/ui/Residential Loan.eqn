equations{ 'fin.res.loan_amount' } = define() {
	return ${system.installed_cost}*${fin.res.debt_fraction}/100; };

equations{ 'fin.res.wacc' } = define() {
	return 100*(
( 
(1+${fin.real_discount_rate}/100)*(1+${fin.inflation_rate}/100)-1)
*
(1-${fin.res.debt_fraction}/100)
+(1-(${fin.federal_tax}/100
*(1-${fin.state_tax}/100)
+${fin.state_tax}/100))
*${fin.res.loan_rate}/100
*${fin.res.debt_fraction}/100); };

