    "Equations": [
        "equations{ 'cp_system_nameplate' } = define() {",
        "\tif (technology() == \"Standalone Battery\") return ${batt_power_discharge_max_kwac} / 1000;",
        "\telse return ${system_capacity}/1000;",
        "};",
        "",
        "equations{ 'ui_system_nameplate_standalone'} = define() {",
        "\tif (technology() == \"Standalone Battery\") return ${cp_system_nameplate};",
        "\telse return 0;",
        "};",
        "",
        "equations{ 'cp_battery_nameplate' } = define() {",
        "\tt = technology();",
        "\tif ( (t=='PV Battery') || (t=='Custom Generation Battery') || (t=='Standalone Battery') )",
        "\t\t{ x = ${batt_power_discharge_max_kwac}/1000; }",
        "\telseif ( t == 'PVWatts Battery' )\t",
        "\t\t{ x = ${batt_simple_kw}/1000; }",
        "\telseif ( t == 'ETES' )",
        "\t\t{ x = ${cp_system_nameplate}; }",
        "\telse",
        "\t\t{ x = 0; }",
        "\treturn x;\t",
        "};",
        "",
        "//capacity credit % from ISO based on battery duration?",
        "//4 hours of duration at ?? MW capacity to qualify for payment",
        "//Sysstem nameplate in AC",
        "//rename system nameplate to nominal bank power when tech = standalone"
    ],
    "Callbacks": [
        "function capacity_payments_ctrl_refresh()",
        "{",
        "\tfixed_payment = (value(\"cp_capacity_payment_type\") == 1);",
        "\tshow(\"cp_payment_units\",!fixed_payment);",
        "\tshow(\"cp_payment_units_dollars\",fixed_payment);",
        "",
        "\t//enable(\"cp_capacity_credit_group\",!fixed_payment);",
        "\tenable(\"cp_capacity_credit_percent\",!fixed_payment);",
        "",
        "\tenable(\"cp_system_nameplate\",!fixed_payment);",
        "\tenable(\"cp_battery_nameplate\",!fixed_payment);",
        "\t",
        "\tcp_payment = value(\"cp_capacity_payment_amount\");",
        "\tshow_escalation = (#cp_payment < 2);",
        "\tenable(\"cp_capacity_payment_esc\",show_escalation);",
        "",
        "\trefresh();",
        "}",
        "",
        "on_load{\"Financial Capacity Payments\"} = define() {",
        "\tcapacity_payments_ctrl_refresh();",
        "\tcapacity_payment_options();",
        "\tproperty('cp_capacity_payment_amount','FixedLength',value('analysis_period'));",
        "};",
        "",
        "on_change{\"cp_capacity_payment_type\"} =",
        "on_change{\"cp_capacity_payment_amount\"} = define()",
        "{",
        "\tcapacity_payments_ctrl_refresh();",
        "};\t",
        "",
        "function capacity_payment_options()",
        "{",
        "\ttech = technology();",
        "\tif (tech == \"Standalone Battery\") {",
        "\t\tshow('cp_system_nameplate', false);",
        "\t\tshow('cp_battery_nameplate', true);",
        "\t\tshow('battery_nameplate_text', false);",
        "\t\tproperty('capacity_payments_text', 'Caption', 'For projects that earn revenue from capacity payments, choose the capacity basis or fixed amount option. For Capacity Basis, the annual capacity payment is the product of the capacity payment amount ($/MW-yr), battery maximum discharge power (MWac), and eligible percent of nameplate (%). For Fixed Amount, the capacity payment is the amount you enter.');",
        "\t}",
        "\telse if (tech == \"PV Battery\" || tech == \"Custom Generation Battery\" || tech == \"PVWatts Battery\") {",
        "\t\tshow('cp_battery_nameplate', true);",
        "\t\tshow('cp_system_nameplate', true);",
        "\t\tproperty('capacity_payments_text', 'Caption', 'For projects that earn revenue from capacity payments, choose the capacity basis or fixed amount option. For Capacity Basis, the annual capacity payment is the product of the capacity payment amount ($/MW-yr), system nameplate capacity (MWdc for PV systems, MWac for other systems), and eligible percent of nameplate (%). For Fixed Amount, the capacity payment is the amount you enter.');",
        "\t\tshow('battery_nameplate_text', true);",
        "\t}",
        "\telse {",
        "\t\tshow('cp_system_nameplate', true);",
        "\t\tshow('cp_battery_nameplate', false);",
        "\t\tproperty('capacity_payments_text', 'Caption', 'For projects that earn revenue from capacity payments, choose the capacity basis or fixed amount option. For Capacity Basis, the annual capacity payment is the product of the capacity payment amount ($/MW-yr), system nameplate capacity (MWdc for PV systems, MWac for other systems), and eligible percent of nameplate (%). For Fixed Amount, the capacity payment is the amount you enter.');",
        "\t\tshow('battery_nameplate_text', false);",
        "\t}",
        "\trefresh();",
        "}",
        ""
    ]
}