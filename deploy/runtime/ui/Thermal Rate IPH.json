    "Callbacks": [
        "",
        "on_load{'Thermal Rate IPH'} = define() {",
        "\tfin = financing();",
        "\tbuy_enable = (fin == \"Commercial\");",
        "\tenable(\"thermal_buy_rate_option\",fin);",
        "\tenable(\"thermal_buy_rate_flat_heat_btu\",fin);",
        "\tenable(\"thermal_timestep_buy_rate_heat_btu\",fin);",
        "\tif (buy_enable) {",
        "\t\ton_change{'thermal_buy_rate_option'}();",
        "\t}",
        "};",
        "",
        "on_change{'thermal_buy_rate_option'} = define() {",
        "\tx = value('thermal_buy_rate_option');",
        "\tenable('thermal_buy_rate_flat_heat_btu',x==0);",
        "\tenable('thermal_timestep_buy_rate_heat_btu',x==1);\r",
        "\tenable('thermal_monthly_buy_rate_heat_btu',x==2);",
        "};",
