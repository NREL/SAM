    ],
    "Callbacks": [
        "function enable_sales_tax() ",
        "{",
        "",
        "\tstr = 'The sales tax rate applies to the total direct cost on the Installation Costs page.';",
        "\tcolor = 'black';",
        "\t",
        "\ttech = technology();",
        "\tif (tech=='Flat Plate PV' || tech=='PVWatts') {",
        "\t\tcapex_cost_choice = value('pv_capex_cost_choice');",
        "\t\tenable('sales_tax_rate', capex_cost_choice!=1);",
        "\t\tif ( capex_cost_choice == 1 ) {",
        "\t\t\tstr = 'Sales tax is disabled for the capital cost curve option on the Installation Costs page. Be sure to include sales tax in the costs you specify on the Installation Costs page.';",
        "\t\t\tcolor = 'black';",
        "\t\t}\t",
        "\t}",
        "",
        "\tproperty( 'capex_table_sales_tax_warning', 'Caption', str );",
        "\tproperty( 'capex_table_sales_tax_warning', 'TextColour', color );",
        "}",
        "",
        "on_load{'Financial Tax and Insurance Rates'} = define() {",
        "\tenable_sales_tax();",
        "};",
        "\t",
        "\t"
    ]
