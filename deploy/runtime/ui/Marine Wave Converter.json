    "Equations": [
        "equations{ 'device_rated_power' } = define () ",
        "{",
        "\tmax_power = 0;",
        "\tmat = ${wave_power_matrix};",
        "\tfor (r=1;r<#mat; r++)",
        "\t\tfor (c=1; c<#mat[0];c++)",
        "\t\t\tif (mat[r][c] > max_power) max_power = mat[r][c];",
        "\treturn max_power;",
        "};",
        "",
        "equations{ 'wave_power_matrix' } = define()",
        "{",
        "\tif (${library_or_input_wec})",
        "\t\treturn ${user_wave_power_matrix};",
        "\telse",
        "\t\treturn ${lib_wave_power_matrix};",
        "};",
        "",
        ""
    ],
    "Callbacks": [
        "function update_controls()",
        "{",
        "\tinput_wec = (value(\"library_or_input_wec\") == 1);",
        "\tshow(\"user_wave_power_matrix\", input_wec);",
        "\tshow(\"user_entered\", input_wec);",
        "\tshow(\"lib_wave_device\", !input_wec);",
        "\tshow(\"lib_wave_power_matrix\", !input_wec);",
        "\tshow('wave_device_name', !input_wec);",
        "\tshow('device_mooring', !input_wec);",
        "\tshow('device_foundation', !input_wec);",
        "\tshow('device_pto_type', !input_wec);",
        "\tshow('device_mass', !input_wec);",
        "\tshow('device_characteristic_diameter', !input_wec);",
        "\tshow('device_material', !input_wec);",
        "\tshow('device_tech_type', !input_wec);",
        "",
        "\trefresh();",
        "}",
        "",
        "on_load{'Marine Wave Converter'}=define()",
        "{",
        "\t//enable(\"lib_wave_power_curve\", false);",
        "\tvalue( 'wave_device_name', value( 'lib_wave_device' ) );",
        "\tupdate_controls();",
        "};",
        "",
        "",
        "on_change{'library_or_input_wec'} = define()",
        "{",
        "\tupdate_controls();",
        "};",
        "",
        "on_change{'lib_wave_device'} = define()",
        "{",
        "\tif( librarygetcurrentselection( 'lib_wave_device' ) != '' )",
        "\t{",
        "\t\tvalue( 'wave_device_name', value('lib_wave_device') );",
        "\t}",
        "};",
        ""
    ]
