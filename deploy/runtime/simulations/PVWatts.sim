on_invoke{'PVWatts'} = define() {
	ssc_copy( 'file_name', 'solar_resource.file' );
	ssc_copy( 'system_size', 'pvwatts.dcrate' );
	ssc_copy( 'derate', 'pvwatts.derate' );
	ssc_copy( 'track_mode', 'pvwatts.array_type' );
	ssc_copy( 'tilt', 'pvwatts.tilt' );
	ssc_copy( 'azimuth', 'pvwatts.azimuth' );
	
	if ( ssc_invoke( 'pvwattsv1' ) )
	{
		sam_copy( 'system.annual.out', 'ac' );
		return true;
	}
	
	return false;
};
