	addpage( [ ['PV Losses'] ], { 'sidebar'='Losses', 'help'='pv_losses' } );

	if (is_btm)
		setup_battery_pages_behind_the_meter_reopt();
	else
		setup_battery_pages_front_of_meter();

	setup_grid_page();
}

