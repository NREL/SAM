function set_om(xl) { 
 
	// fixed and variable O&M costs 
 
	omf = value('om_fixed'); 
	omc = value('om_capacity'); 
	omp = value('om_production'); 
		 
	if ( #omf == 1 )  
	{ 
		send_excel(       xl, 'om_fixed'); 
		send_excel(       xl, 'om_fixed_escal'); 
	} 
	else  
	{ 
		xl_set( xl, omf, 'AnnualOMFixed'); 
		xl_set( xl, 'n/a', 'om_fixed'); 
		xl_set( xl, 'n/a', 'om_fixed_escal'); 
	}	 
	 
	if ( #omc == 1 )  
	{  
		send_excel( xl, 'om_capacity'); 
		send_excel( xl, 'om_capacity_escal'); 
	} 
	else  
	{ 
		xl_set( xl, omc, 'AnnualOMCapacity'); 
		xl_set( xl, 'n/a', 'om_capacity'); 
		xl_set( xl, 'n/a', 'om_capacity_escal'); 
	}	 
	 
	if ( #omp == 1 ) {	 
		send_excel( xl, 'om_production'); 
		send_excel( xl, 'om_production_escal'); 
	} 
	else  
	{ 
		xl_set( xl, omp, 'AnnualOMProduction'); 
		xl_set( xl, 'n/a', 'om_production'); 
		xl_set( xl, 'n/a', 'om_production_escal'); 
	} 
	 
	// battery replacment costs and retail electricity purchases
	tech = technology(); 
	fin = financing(); 
	if ( is_batt_tech() ) 
	{
		send_excel( xl, 'batt_replacement_option' );
		xl_set( xl, output('cf_battery_replacement_cost'), 'cf_battery_replacement_cost' ); 
		// retail purchases to charge battery, only for single owner as of 2020.2.29
		if ( fin == 'Single Owner' )
		{
			xl_set( xl, output('cf_utility_bill'), 'cf_utility_bill' ); 
		}
	}
	
	if ( tech == 'Fuel Cell' && ( /*fin == 'Commercial' ||*/ fin == 'Single Owner' ) ) // fuel cell only available for commercial and single owner as of SAM 2020.2.29
	{
		xl_set( xl, 'Yes', 'IsFuelCell' );
		xl_set( xl, output('cf_om_fixed1_expense'), 'cf_om_fixed1_expense' );
		xl_set( xl, output('cf_om_fixed2_expense'), 'cf_om_fixed2_expense' ) ;
		xl_set( xl, output('cf_om_capacity1_expense'), 'cf_om_capacity1_expense' );
		xl_set( xl, output('cf_om_capacity2_expense'), 'cf_om_capacity2_expense' ) ;
		xl_set( xl, output('cf_fuelcell_replacement_cost'), 'cf_fuelcell_replacement_cost' ); 
		xl_set( xl, output('cf_om_fuel_expense'), 'cf_om_fuel_expense' ); 
	}
	else { xl_set( xl, 'Fuel cell O&M costs not yet implemented. Cash flow send-to-Excel for Commercial does not acount for fuel cell-specific operating costs.', 'om_fixed1' ); } // in case we add other financial models for fuel cell
	
}

