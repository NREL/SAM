function cf_levelized_metrics()
{
	fin = financing();
	tech = technology();
	has_roe = ( fin == 'Single Owner' || fin == 'Host Developer' );
	has_ppa_revenue = ( fin != 'Community Solar');
	cfline('AFTER-TAX LEVELIZED COE' + ? has_ppa_revenue[ '', ' AND PPA PRICE'], -1); 
	cfline('cf_annual_costs', 0);
	if ( has_ppa_revenue ) { cfline('cf_energy_value', 0); }
	cfline('cf_energy_net', 0);  
	cfline( '' ); 
	cfline('npv_annual_costs',0 ); 
	cfline('npv_energy_nom', 0); 
	cfline('lcoe_nom',2); 
	
