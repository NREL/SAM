			old_replacement_cost = oldvalue('batt_replacement_cost');
			old_escalation = oldvalue('batt_replacement_cost_escal');
			value('om_replacement_cost1', [old_replacement_cost], 'Updated battery replacement cost input, now on System Costs page.');
			value('om_replacement_cost_escal', old_escalation);
